@prefix com: <http://sweetontology.net/humanCommerce/> .
@prefix human: <http://sweetontology.net/human/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prop: <http://sweetontology.net/prop/> .
@prefix prot: <http://sweetontology.net/humanEnvirConservation/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix realm: <http://sweetontology.net/realm/> .
@prefix rela: <http://sweetontology.net/rela/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sweetontology.net/humanEnvirConservation>
  rdf:type owl:Ontology ;
  rdfs:label "SWEET Ontology" ;
  owl:imports <http://sweetontology.net/human> ;
  owl:imports <http://sweetontology.net/humanCommerce> ;
  owl:imports <http://sweetontology.net/prop> ;
  owl:imports <http://sweetontology.net/realm> ;
  owl:imports <http://sweetontology.net/rela> ;
  owl:versionInfo "3.1.0" ;
.
prot:Conservation
  rdf:type owl:Class ;
  rdfs:subClassOf com:Management ;
.
prot:HabitatManagement
  rdf:type owl:Class ;
  rdfs:subClassOf prot:LandManagement ;
.
prot:LandManagement
  rdf:type owl:Class ;
  rdfs:subClassOf com:Management ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty rela:hasRealm ;
      owl:someValuesFrom realm:Land ;
    ] ;
.
prot:LandPreservation
  rdf:type owl:Class ;
  rdfs:subClassOf prot:LandManagement ;
.
prot:LandTenure
  rdf:type owl:Class ;
  rdfs:subClassOf prot:LandManagement ;
.
prot:Protection
  rdf:type owl:Class ;
  rdfs:subClassOf prot:Sustainability ;
  rdfs:subClassOf prop:Role ;
.
prot:Stewardship
  rdf:type owl:Class ;
  rdfs:subClassOf com:Management ;
.
prot:Sustainability
  rdf:type owl:Class ;
.
prot:Sustainable
  rdf:type owl:Class ;
  rdfs:subClassOf human:HumanActivity ;
  owl:equivalentClass prot:Sustainability ;
.
