@prefix huma: <http://sweetontology.net/human/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix repr: <http://sweetontology.net/repr/> .
@prefix res: <http://sweetontology.net/humanResearch/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sweetontology.net/humanResearch>
  rdf:type owl:Ontology ;
  rdfs:label "SWEET Ontology" ;
  owl:imports <http://sweetontology.net/human> ;
  owl:imports <http://sweetontology.net/repr> ;
  owl:versionInfo "3.1.0" ;
.
res:Analysis
  rdf:type owl:Class ;
  rdfs:subClassOf res:Research ;
.
res:AppliedResearch
  rdf:type owl:Class ;
  rdfs:subClassOf res:Research ;
.
res:Assessment
  rdf:type owl:Class ;
  rdfs:subClassOf res:Investigation ;
.
res:Campaign
  rdf:type owl:Class ;
  rdfs:subClassOf res:ExperimentActivity ;
.
res:Correction
  rdf:type owl:Class ;
  rdfs:subClassOf res:ExperimentActivity ;
.
res:Difference
  rdf:type owl:Class ;
  rdfs:subClassOf res:ExperimentActivity ;
.
res:EndToEndEnvironment
  rdf:type owl:Class ;
  rdfs:subClassOf res:ResearchSetting ;
  owl:disjointWith res:LaboratoryEnvironment ;
.
res:Evidence
  rdf:type owl:Class ;
  rdfs:subClassOf res:Assessment ;
.
res:Experiment
  rdf:type owl:Class ;
  rdfs:subClassOf res:Investigation ;
.
res:ExperimentActivity
  rdf:type owl:Class ;
  rdfs:subClassOf res:Experiment ;
.
res:HypothesisTest
  rdf:type owl:Class ;
  rdfs:subClassOf res:ExperimentActivity ;
.
res:Investigate
  rdf:type owl:Class ;
.
res:Investigation
  rdf:type owl:Class ;
  rdfs:subClassOf res:Research ;
  owl:equivalentClass res:Investigate ;
.
res:LaboratoryEnvironment
  rdf:type owl:Class ;
  rdfs:subClassOf res:ResearchSetting ;
.
res:MissionTestedEnvironment
  rdf:type owl:Class ;
  rdfs:subClassOf res:ResearchSetting ;
  owl:disjointWith res:LaboratoryEnvironment ;
.
res:Monitor
  rdf:type owl:Class ;
  rdfs:subClassOf res:Research ;
  owl:equivalentClass res:Monitoring ;
.
res:Monitoring
  rdf:type owl:Class ;
.
res:Observation
  rdf:type owl:Class ;
  rdfs:subClassOf res:Investigation ;
  owl:equivalentClass res:Observe ;
.
res:Observe
  rdf:type owl:Class ;
.
res:OperationalEnvironment
  rdf:type owl:Class ;
  rdfs:subClassOf res:ResearchSetting ;
  owl:disjointWith res:LaboratoryEnvironment ;
.
res:Project
  rdf:type owl:Class ;
  rdfs:subClassOf res:Research ;
.
res:Proof
