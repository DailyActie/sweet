.
res:ProofOfConcept
  rdf:type owl:Class ;
  rdfs:subClassOf res:Proof ;
  rdfs:subClassOf res:Research ;
.
res:Publication
  rdf:type owl:Class ;
  rdfs:subClassOf res:Research ;
.
res:Realization
  rdf:type owl:Class ;
  rdfs:subClassOf res:ExperimentActivity ;
.
res:RepresentativeEnvironment
  rdf:type owl:Class ;
  rdfs:subClassOf res:ResearchSetting ;
  owl:disjointWith res:LaboratoryEnvironment ;
.
res:Research
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
res:ResearchSetting
  rdf:type owl:Class ;
  rdfs:subClassOf res:Research ;
.
res:Residual
  rdf:type owl:Class ;
  rdfs:subClassOf res:Research ;
.
res:Result
  rdf:type owl:Class ;
  rdfs:subClassOf res:Research ;
.
res:Sample
  rdf:type owl:Class ;
  rdfs:subClassOf res:ExperimentActivity ;
.
res:Validation
  rdf:type owl:Class ;
  rdfs:subClassOf res:ExperimentActivity ;
.
res:Variable
  rdf:type owl:Class ;
