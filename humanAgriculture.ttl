@prefix agri: <http://sweetontology.net/humanAgriculture/> .
@prefix anim: <http://sweetontology.net/matrAnimal/> .
@prefix comm: <http://sweetontology.net/humanCommerce/> .
@prefix comp: <http://sweetontology.net/matrCompound/> .
@prefix crela: <http://sweetontology.net/relaChemical/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix screla: <http://sweetontology.net/relaSci/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sweetontology.net/humanAgriculture>
  rdf:type owl:Ontology ;
  rdfs:label "SWEET Ontology" ;
  owl:imports <http://sweetontology.net/humanCommerce> ;
  owl:imports <http://sweetontology.net/matrAnimal> ;
  owl:imports <http://sweetontology.net/matrCompound> ;
  owl:imports <http://sweetontology.net/relaChemical> ;
  owl:imports <http://sweetontology.net/relaSci> ;
  owl:versionInfo "3.1.0" ;
.
agri:Agriculture
  rdf:type owl:Class ;
  rdfs:subClassOf comm:LandUse ;
  rdfs:subClassOf comm:Production ;
.
agri:AnimalProduction
  rdf:type owl:Class ;
  rdfs:subClassOf agri:Agriculture ;
.
agri:Apiculture
  rdf:type owl:Class ;
  rdfs:subClassOf agri:AnimalProduction ;
.
agri:Aquaculture
  rdf:type owl:Class ;
  rdfs:subClassOf agri:Agriculture ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom anim:MarineAnimal ;
      owl:onProperty screla:hasSpecies ;
    ] ;
.
agri:Beekeeping
  rdf:type owl:Class ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom anim:Bee ;
      owl:onProperty screla:hasSpecies ;
    ] ;
  owl:equivalentClass agri:Apiculture ;
.
agri:Breeding
  rdf:type owl:Class ;
  rdfs:subClassOf agri:AnimalProduction ;
.
agri:CropProduction
  rdf:type owl:Class ;
  rdfs:subClassOf agri:PlantProduction ;
.
agri:Cultivation
  rdf:type owl:Class ;
  rdfs:subClassOf agri:Agriculture ;
.
agri:DripIrrigation
  rdf:type owl:Class ;
  rdfs:subClassOf agri:Agriculture ;
.
agri:FishProduction
  rdf:type owl:Class ;
  rdfs:subClassOf agri:AnimalProduction ;
  rdfs:subClassOf agri:Aquaculture ;
.
agri:Fishing
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Production ;
.
agri:Harvesting
  rdf:type owl:Class ;
  rdfs:subClassOf agri:PlantProduction ;
.
agri:Horticulture
  rdf:type owl:Class ;
  rdfs:comment "Horticulture is the art and science of the cultivation of plants"@en ;
  rdfs:subClassOf agri:Agriculture ;
.
agri:Irrigation
  rdf:type owl:Class ;
  rdfs:subClassOf agri:SoilCultivation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue comp:Water ;
      owl:onProperty crela:hasSubstance ;
    ] ;
.
agri:Logging
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Production ;
.
agri:Overfishing
  rdf:type owl:Class ;
  rdfs:subClassOf agri:Fishing ;
.
agri:PlantProduction
  rdf:type owl:Class ;
  rdfs:subClassOf agri:Agriculture ;
.
agri:Sericulture
  rdf:type owl:Class ;
  rdfs:subClassOf agri:AnimalProduction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom anim:Silkworm ;
      owl:onProperty screla:hasSpecies ;
    ] ;
.
agri:ShellfishProduction
  rdf:type owl:Class ;
  rdfs:subClassOf agri:AnimalProduction ;
  rdfs:subClassOf agri:Aquaculture ;
.
agri:SoilCultivation
  rdf:type owl:Class ;
  rdfs:subClassOf agri:Agriculture ;
.
