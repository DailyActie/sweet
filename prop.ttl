# baseURI: http://sweetontology.net/prop
# imports: http://sweetontology.net/humanKnowledgeDomain
# imports: http://sweetontology.net/procPhysical
# imports: http://sweetontology.net/rela
# imports: http://sweetontology.net/relaHuman
# prefix: prop

@prefix hrela: <http://sweetontology.net/relaHuman> .
@prefix know: <http://sweetontology.net/humanKnowledgeDomain> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix proc: <http://sweetontology.net/procPhysical> .
@prefix prop: <http://sweetontology.net/prop#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rela: <http://sweetontology.net/rela> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sweetontology.net/prop>
  rdf:type owl:Ontology ;
  rdfs:label "SWEET Ontology" ;
  owl:imports <http://sweetontology.net/humanKnowledgeDomain> ;
  owl:imports <http://sweetontology.net/procPhysical> ;
  owl:imports <http://sweetontology.net/rela> ;
  owl:imports <http://sweetontology.net/relaHuman> ;
  owl:versionInfo "3.1.0" ;
.
prop:Accuracy
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Metric ;
.
prop:AngularMotionProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:MotionProperty ;
.
prop:BiologicalProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Property ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <http://sweetontology.net/relaHuman#studyOf> ;
      owl:someValuesFrom <http://sweetontology.net/humanKnowledgeDomain#Biology> ;
    ] ;
.
prop:ChemicalProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Property ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <http://sweetontology.net/relaHuman#studyOf> ;
      owl:someValuesFrom <http://sweetontology.net/humanKnowledgeDomain#Chemistry> ;
    ] ;
.
prop:Distortion
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Error ;
.
prop:Efficiency
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Measure ;
.
prop:ElectromagneticProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:PhysicalProperty ;
.
prop:ElectromagneticRadiationProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:PhysicalProperty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom <http://sweetontology.net/procPhysical#RadiativeTransfer> ;
      owl:onProperty <http://sweetontology.net/rela#hasProcess> ;
    ] ;
.
prop:Error
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Metric ;
.
prop:Fideltiy
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Measure ;
.
prop:FlowProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:SystemProperty ;
.
prop:FluidProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:PhysicalProperty ;
.
prop:GeologicalProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Property ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <http://sweetontology.net/relaHuman#studyOf> ;
      owl:someValuesFrom <http://sweetontology.net/humanKnowledgeDomain#Geology> ;
    ] ;
.
prop:HydrogeologicalProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:GeologicalProperty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <http://sweetontology.net/relaHuman#studyOf> ;
      owl:someValuesFrom <http://sweetontology.net/humanKnowledgeDomain#Hydrology> ;
    ] ;
.
prop:Measure
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Property ;
.
prop:Metric
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Measure ;
.
prop:MotionProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:PhysicalProperty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <http://sweetontology.net/relaHuman#studyOf> ;
      owl:someValuesFrom <http://sweetontology.net/humanKnowledgeDomain#Dynamics> ;
    ] ;
.
prop:PhysicalProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Property ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <http://sweetontology.net/relaHuman#studyOf> ;
      owl:someValuesFrom <http://sweetontology.net/humanKnowledgeDomain#Physics> ;
    ] ;
.
prop:Precision
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Metric ;
.
prop:Property
  rdf:type owl:Class ;
.
prop:RadiationMediumInteractionProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:ElectromagneticRadiationProperty ;
.
prop:ReactionProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:ChemicalProperty ;
.
prop:Role
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Property ;
.
prop:Sensitivity
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Measure ;
.
prop:StatisticalSummary
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Property ;
.
prop:StockProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:SystemProperty ;
.
prop:SystemProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:Property ;
.
prop:ThermodynamicProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:PhysicalProperty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <http://sweetontology.net/relaHuman#studyOf> ;
      owl:someValuesFrom <http://sweetontology.net/humanKnowledgeDomain#Thermodynamics> ;
    ] ;
.
prop:WaveProperty
  rdf:type owl:Class ;
  rdfs:subClassOf prop:PhysicalProperty ;
.
