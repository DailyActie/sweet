<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY phys "http://sweet.jpl.nasa.gov/2.0/phys.owl">
  <!ENTITY ener "http://sweet.jpl.nasa.gov/2.0/physEnergy.owl">
  <!ENTITY pres "http://sweet.jpl.nasa.gov/2.0/physPressure.owl">
  <!ENTITY role "http://sweet.jpl.nasa.gov/2.0/sciRole.owl">
  <!ENTITY dir "http://sweet.jpl.nasa.gov/2.0/spaceDirection.owl">
  <!ENTITY top "http://sweet.jpl.nasa.gov/2.0/top.owl">
  <!ENTITY res "http://sweet.jpl.nasa.gov/2.0/sciResearch.owl">
  <!ENTITY solid "http://sweet.jpl.nasa.gov/2.0/physSolid.owl">
  <!ENTITY trans "http://sweet.jpl.nasa.gov/2.0/physTransport.owl">
  <!ENTITY dist "http://sweet.jpl.nasa.gov/2.0/spaceDistribution.owl">
  <!ENTITY cont "http://sweet.jpl.nasa.gov/2.0/physContinuumMechanics.owl">
  <!ENTITY obj "http://sweet.jpl.nasa.gov/2.0/spaceObject.owl">
  <!ENTITY state "http://sweet.jpl.nasa.gov/2.0/chemState.owl">
  <!ENTITY units "http://sweet.jpl.nasa.gov/2.0/sciUnits.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&cont;"
         xmlns:owl="&owl;"
         xmlns:res="&res;"
         xmlns:obj="&obj;"
         xmlns:phys="&phys;"
         xmlns:ener="&ener;"
         xmlns:pres="&pres;"
         xmlns:role="&role;"
         xmlns:top="&top;"
         xmlns:dir="&dir;"
         xmlns:cont="&cont;"
         xmlns:trans="&trans;"
         xmlns:units="&units;"
         xmlns:dist="&dist;"
         xmlns:state="&state;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.0">
     <owl:imports rdf:resource="&phys;"/>
     <owl:imports rdf:resource="&ener;"/>
     <owl:imports rdf:resource="&role;"/>
     <owl:imports rdf:resource="&res;"/>
     <owl:imports rdf:resource="&top;"/>
     <owl:imports rdf:resource="&pres;"/>
     <owl:imports rdf:resource="&obj;"/>
     <owl:imports rdf:resource="&dir;"/>
     <owl:imports rdf:resource="&units;"/>
     <owl:imports rdf:resource="&trans;"/>
     <owl:imports rdf:resource="&dist;"/>
     <owl:imports rdf:resource="&state;"/>
  </owl:Ontology>

<!-- Properties -->
  <owl:Class rdf:about="#BulkModulus">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalProperty"/>
  </owl:Class>

  <owl:Class rdf:about="#CoefficientOfThermalExpansion">
    <rdfs:subClassOf rdf:resource="#MaterialProperty"/>
    <owl:equivalentClass rdf:resource="#ThermalExpansionCoefficient"/>
  </owl:Class>
  <owl:Class rdf:about="#ThermalExpansionCoefficient"/>

  <owl:Class rdf:about="#DispersionCoeffienct">
    <rdfs:subClassOf rdf:resource="#MaterialProperty"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&res;#measureOf" />
        <owl:someValuesFrom rdf:resource="#Dispersivity" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Dispersivity">
    <rdfs:subClassOf rdf:resource="#MaterialProperty"/>
  </owl:Class>

  <owl:Class rdf:about="#Friction">
    <rdfs:subClassOf rdf:resource="&phys;#Force"/>
  </owl:Class>

  <owl:Class rdf:about="#MediumProperty">
    <rdfs:subClassOf rdf:resource="&phys;#IntensiveProperty"/>
    <owl:equivalentClass rdf:resource="#MaterialProperty"/>
  </owl:Class>
  <owl:Class rdf:about="#MaterialProperty"/>

  <owl:Class rdf:about="#Roughness">
    <rdfs:subClassOf rdf:resource="#MaterialProperty"/>
  </owl:Class>

  <owl:Class rdf:about="#ShearModulus">
    <rdfs:subClassOf rdf:resource="#MaterialProperty"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&dir;#hasDirection" />
        <owl:someValuesFrom rdf:resource="&dir;#Shear" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ShearStress">
    <rdfs:subClassOf rdf:resource="&pres;#Stress"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&dir;#hasDirection" />
        <owl:someValuesFrom rdf:resource="&dir;#Shear" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Strain">
    <rdfs:subClassOf rdf:resource="&phys;#ExtensiveProperty"/>
  </owl:Class>

  <owl:Class rdf:about="#TensileStress">
    <rdfs:subClassOf rdf:resource="&pres;#Stress"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&dir;#hasDirection" />
        <owl:someValuesFrom rdf:resource="&dir;#Perpendicular" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#ShearStress"/>
    <owl:equivalentClass rdf:resource="#NormalStress"/>
  </owl:Class>
  <owl:Class rdf:about="#NormalStress"/>

  <owl:Class rdf:about="#Tension">
    <rdfs:subClassOf rdf:resource="&phys;#Force"/>
  </owl:Class>

  <owl:Class rdf:about="#YoungsModulus">
    <rdfs:subClassOf rdf:resource="#MaterialProperty"/>
    <owl:equivalentClass rdf:resource="#ModulusOfElasticity"/>
  </owl:Class>
  <owl:Class rdf:about="#ModulusOfElasticity"/>

<!-- Knowledge domains -->
  <owl:Class rdf:about="#Rheology">
    <rdfs:subClassOf rdf:resource="&phys;#Physics"/>
  </owl:Class>

<!-- State -->
  <owl:Class rdf:about="#Conduction">
    <rdfs:subClassOf rdf:resource="&ener;#EnergyTransfer"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&role;#hasRole" />
        <owl:someValuesFrom rdf:resource="#Conduct" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Transport of energy (charge) solely as a consequence of random motions of individual molecules (ions, electrons) not moving together in coherent groups.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Conduct">
    <rdfs:subClassOf rdf:resource="&role;#Role"/>
  </owl:Class>

  <owl:Class rdf:about="#Conductor">
    <rdfs:subClassOf rdf:resource="&top;#Matter"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&role;#hasRole" />
        <owl:someValuesFrom rdf:resource="#Conduct" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Insulation">
    <rdfs:subClassOf rdf:resource="&ener;#EnergyTransfer"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&role;#hasRole" />
        <owl:someValuesFrom rdf:resource="#Insulate" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Insulate">
    <rdfs:subClassOf rdf:resource="&role;#Role"/>
  </owl:Class>

  <owl:Class rdf:about="#Insulator">
    <rdfs:subClassOf rdf:resource="&top;#Matter"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&role;#hasRole" />
        <owl:someValuesFrom rdf:resource="#Insulate" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Conductance">
    <rdfs:subClassOf rdf:resource="#MediumProperty"/>
  </owl:Class>

  <owl:Class rdf:about="#Conductivity">
    <rdfs:subClassOf rdf:resource="#MediumProperty"/>
  </owl:Class>

  <owl:Class rdf:about="#SpecificConductance">
    <rdfs:subClassOf rdf:resource="#MediumProperty"/>
  </owl:Class>

  <owl:Class rdf:about="#SpecificConductivity">
    <rdfs:subClassOf rdf:resource="#MediumProperty"/>
  </owl:Class>

</rdf:RDF>
