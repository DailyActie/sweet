<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY dist "http://sweet.jpl.nasa.gov/2.0/spaceDistribution.owl">  
  <!ENTITY sol "http://sweet.jpl.nasa.gov/2.0/mathSolution.owl">  
  <!ENTITY space "http://sweet.jpl.nasa.gov/2.0/spaceExtent.owl">  
  <!ENTITY func "http://sweet.jpl.nasa.gov/2.0/mathFunction.owl">  
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&dist;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:sol="&sol;"
         xmlns:dist="&dist;"
         xmlns:space="&space;"
         xmlns:func="&func;"
>
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
     <owl:imports rdf:resource="&func;"/>
     <owl:imports rdf:resource="&sol;"/>
     <owl:imports rdf:resource="&space;"/>
  </owl:Ontology>

<!-- Classes -->
  <owl:Class rdf:about="#AngularDistribution">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>
  
  <owl:Class rdf:about="#Anisotropic">
    <rdfs:subClassOf rdf:resource="#AngularDistribution"/>
    <owl:disjointWith rdf:resource="#Isotropic" />
  </owl:Class>

  <owl:Class rdf:about="#Anisotropy">
    <rdfs:subClassOf rdf:resource="#AngularDistribution"/>
    <owl:disjointWith rdf:resource="#Isotropy" />
  </owl:Class>

  <owl:Class rdf:about="#Asymmetry">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
    <owl:disjointWith rdf:resource="#Symmetry" />
    <owl:equivalentClass rdf:resource="#Asymmetric"/>
  </owl:Class>
  <owl:Class rdf:about="#Asymmetric"/>

  <owl:Class rdf:about="#Biaxial">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>
  
  <owl:Class rdf:about="#CenterOfMass">
    <rdfs:subClassOf rdf:resource="&space;#SpatialMeasure"/>
  </owl:Class>

  <owl:Class rdf:about="#Cluster">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Configuration">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Confined">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Contained">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Containment">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Contract">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
    <owl:equivalentClass rdf:resource="#Contracting"/>
    <owl:disjointWith rdf:resource="#Expand" />
  </owl:Class>
  <owl:Class rdf:about="#Contracting"/>

  <owl:Class rdf:about="#Expand">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
    <owl:equivalentClass rdf:resource="#Expanding"/>
  </owl:Class>
  <owl:Class rdf:about="#Expanding"/>

  <owl:Class rdf:about="#Isotropic">
    <rdfs:subClassOf rdf:resource="#AngularDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Isotropy">
    <rdfs:subClassOf rdf:resource="#AngularDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Lattice">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Meander">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
    <owl:equivalentClass rdf:resource="#Meandering"/>
  </owl:Class>
  <owl:Class rdf:about="#Meandering"/>

  <owl:Class rdf:about="#Patch">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Spiral">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>
  
  <owl:Class rdf:about="#Triaxial">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>
  
  <owl:Class rdf:about="#AspectRatio">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Shape">
    <rdfs:subClassOf rdf:resource="&space;#SpatialMeasure"/>
    <rdfs:comment xml:lang="en">Description of the form of an object</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Round">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Rounded">
   <rdfs:subClassOf rdf:resource="#Shape"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&sol;#approximates" />
        <owl:allValuesFrom rdf:resource="#Round" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Bladed">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Compact">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Flat">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Flattend">
   <rdfs:subClassOf rdf:resource="#Shape"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&sol;#approximates" />
        <owl:allValuesFrom rdf:resource="#Flat" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Fretted">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Grooved">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Scalloped">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Short">
    <rdfs:subClassOf rdf:resource="#Shape"/>
    <owl:disjointWith rdf:resource="#Long" />
  </owl:Class>

  <owl:Class rdf:about="#Twisted">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Long">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Elongated">
   <rdfs:subClassOf rdf:resource="#Shape"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&sol;#approximates" />
        <owl:allValuesFrom rdf:resource="#Long" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Wide">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Narrow">
   <rdfs:subClassOf rdf:resource="#Shape"/>
    <owl:disjointWith rdf:resource="#Wide" />
  </owl:Class>

  <owl:Class rdf:about="#Braided">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Branched">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Concealed">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Cutoff">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Extant">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Sharp">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Prolate">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Oblate">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Ridged">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Sheet">
   <rdfs:subClassOf rdf:resource="#Shape"/>
  </owl:Class>

  <owl:Class rdf:about="#Sparse">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>
  
  <owl:Class rdf:about="#SpatialDistribution">
    <rdfs:subClassOf rdf:resource="&func;#DistributionFunction"/>
  </owl:Class>

  <owl:Class rdf:about="#Spread">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
  </owl:Class>

  <owl:Class rdf:about="#Symmetry">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
    <owl:equivalentClass rdf:resource="#Symmetric"/>
  </owl:Class>
  <owl:Class rdf:about="#Symmetric"/>

  <owl:Class rdf:about="#Semiconfined">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
    <owl:disjointWith rdf:resource="#Confined" />
    <owl:disjointWith rdf:resource="#Unconfined" />
  </owl:Class>

  <owl:Class rdf:about="#Unconfined">
    <rdfs:subClassOf rdf:resource="#SpatialDistribution"/>
    <owl:disjointWith rdf:resource="#Confined" />
  </owl:Class>

  <owl:ObjectProperty rdf:about="#hasSpatialDistribution">
    <rdfs:range rdf:resource="#SpatialDistribution"/>
  </owl:ObjectProperty>

</rdf:RDF>
