<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
   <!ENTITY planet "http://sweet.jpl.nasa.gov/2.0/astroPlanet.owl">  
   <!ENTITY water "http://sweet.jpl.nasa.gov/2.0/chemWater.owl">  
   <!ENTITY ocean "http://sweet.jpl.nasa.gov/2.0/ocean.owl">  
   <!ENTITY ice "http://sweet.jpl.nasa.gov/2.0/oceanIce.owl">  
   <!ENTITY owl "http://www.w3.org/2002/07/owl#">
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&ice;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
	 xmlns:xsd = "&xsd;"
	 xmlns:planet="&planet;"
	 xmlns:water="&water;"
	 xmlns:ocean="&ocean;"
    	 xmlns:ice="&ice;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
     <owl:imports rdf:resource="&planet;"/>     
     <owl:imports rdf:resource="&water;"/>     
     <owl:imports rdf:resource="&ocean;"/>     
  </owl:Ontology>

  <owl:Class rdf:about="#CongelationIce">
    <rdfs:subClassOf rdf:resource="#SeaIce" />
  </owl:Class>

  <owl:Class rdf:about="#DriftIce">
    <rdfs:subClassOf rdf:resource="#SeaIce" />
  </owl:Class>

  <owl:Class rdf:about="#FastIce">
    <rdfs:subClassOf rdf:resource="#SeaIce" />
    <owl:disjointWith rdf:resource="#DriftIce"/>
  </owl:Class>

  <owl:Class rdf:about="#Floe">
    <rdfs:subClassOf rdf:resource="#SeaIce"/>
    <owl:equivalentClass rdf:resource="#IceFloe"/>
  </owl:Class>
  <owl:Class rdf:about="#IceFloe"/>

  <owl:Class rdf:about="#FrazilIce">
    <rdfs:subClassOf rdf:resource="#SeaIce" />
  </owl:Class>

  <owl:Class rdf:about="#Iceberg">
    <rdfs:subClassOf rdf:resource="#SeaIce" />
  </owl:Class>

  <owl:Class rdf:about="#IceField">
    <rdfs:subClassOf rdf:resource="#SeaIce" />
  </owl:Class>

  <owl:Class rdf:about="#PackIce">
    <rdfs:subClassOf rdf:resource="#DriftIce" />
  </owl:Class>
  
  <owl:Class rdf:about="#SeaIce">
    <rdfs:subClassOf rdf:resource="&water;#Ice" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&planet;#hasPlanetaryRealm"/>
        <owl:someValuesFrom rdf:resource="&planet;#Ocean"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Specifically, ice formed by the freezing of seawater; as opposed, principally, to land ice.  Generally, any ice floating in the sea.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#SeasonalIce">
    <rdfs:subClassOf rdf:resource="#SeaIce" />
  </owl:Class>

</rdf:RDF>
