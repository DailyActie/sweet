<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY struc "http://sweet.jpl.nasa.gov/2.0/humanStructure.owl">
  <!ENTITY hum "http://sweet.jpl.nasa.gov/2.0/human.owl">
  <!ENTITY trans "http://sweet.jpl.nasa.gov/2.0/humanTransport.owl">
  <!ENTITY comm "http://sweet.jpl.nasa.gov/2.0/humanCommerce.owl">
  <!ENTITY phys "http://sweet.jpl.nasa.gov/2.0/phys.owl">
  <!ENTITY stat "http://sweet.jpl.nasa.gov/2.0/mathStatistics.owl">
  <!ENTITY time "http://sweet.jpl.nasa.gov/2.0/time.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&trans;"
         xmlns:hum="&hum;"
         xmlns:trans="&trans;"
         xmlns:struc="&struc;"
         xmlns:comm="&comm;"
         xmlns:phys="&phys;"
         xmlns:stat="&stat;"
         xmlns:time="&time;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.0">
     <owl:imports rdf:resource="&hum;"/>   
     <owl:imports rdf:resource="&struc;"/>   
     <owl:imports rdf:resource="&comm;"/>   
     <owl:imports rdf:resource="&phys;"/>   
     <owl:imports rdf:resource="&stat;"/>   
     <owl:imports rdf:resource="&time;"/>   
  </owl:Ontology>

  <owl:Class rdf:about="#Evacuation">
    <rdfs:subClassOf rdf:resource="&hum;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Navigation">
    <rdfs:subClassOf rdf:resource="&hum;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Transit">
    <rdfs:subClassOf rdf:resource="#Transportation"/>
  </owl:Class>

  <owl:Class rdf:about="#Transport">
    <rdfs:subClassOf rdf:resource="&hum;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Transportation">
    <rdfs:subClassOf rdf:resource="&hum;#HumanActivity"/>
  </owl:Class>

<!-- Equipment -->
  <owl:Class rdf:about="#EmergencyVehicle">
    <rdfs:subClassOf rdf:resource="#Vehicle"/>
  </owl:Class>

  <owl:Class rdf:about="#NavigationEquipment">
    <rdfs:subClassOf rdf:resource="&comm;#Equipment"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Navigation"/>
        <owl:onProperty rdf:resource="&hum;#hasActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#TransportationEquipment">
    <rdfs:subClassOf rdf:resource="&comm;#Equipment"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Transportation"/>
        <owl:onProperty rdf:resource="&hum;#hasActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#TransportationFacility">
    <rdfs:subClassOf rdf:resource="&struc;#Facility"/>
  </owl:Class>

  <owl:Class rdf:about="#Vehicle">
    <rdfs:subClassOf rdf:resource="#TransportationEquipment"/>
  </owl:Class>

<!-- Properties -->
  <owl:Class rdf:about="#FuelCapacity">
    <rdfs:subClassOf rdf:resource="&phys;#Capacity"/>
  </owl:Class>

  <owl:Class rdf:about="#ArrivalTime">
    <rdfs:subClassOf rdf:resource="&time;#Time"/>
  </owl:Class>

  <owl:Class rdf:about="#DepartureTime">
    <rdfs:subClassOf rdf:resource="&time;#Time"/>
  </owl:Class>

  <owl:Class rdf:about="#EstimatedArrivalTime">
    <rdfs:subClassOf rdf:resource="&time;#Time"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#ArrivalTime"/>
        <owl:onProperty rdf:resource="&stat;#estimateOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#EstimatedDepartureTime">
    <rdfs:subClassOf rdf:resource="&time;#Time"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#DepartureTime"/>
        <owl:onProperty rdf:resource="&stat;#estimateOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:ObjectProperty rdf:about="#hasFuelCapacity">
    <rdfs:domain rdf:resource="#Vehicle" />
    <rdfs:range rdf:resource="#FuelCapacity" />
  </owl:ObjectProperty>

<!-- Ground Travel -->
  <owl:Class rdf:about="#Car">
    <rdfs:subClassOf rdf:resource="#Vehicle"/>
  </owl:Class>

  <owl:Class rdf:about="#Rover">
    <rdfs:subClassOf rdf:resource="#Vehicle"/>
  </owl:Class>

  <owl:Class rdf:about="#Trailer">
    <rdfs:subClassOf rdf:resource="#Vehicle"/>
  </owl:Class>

  <owl:Class rdf:about="#Train">
    <rdfs:subClassOf rdf:resource="#Vehicle"/>
  </owl:Class>

  <owl:Class rdf:about="#Truck">
    <rdfs:subClassOf rdf:resource="#Vehicle"/>
  </owl:Class>

<!-- Water Travel -->
  <owl:Class rdf:about="#Ship">
    <rdfs:subClassOf rdf:resource="#Vehicle"/>
  </owl:Class>

<!-- Underground -->
  <owl:Class rdf:about="#Pipeline">
    <rdfs:subClassOf rdf:resource="#TransportationEquipment"/>
  </owl:Class>

</rdf:RDF>
