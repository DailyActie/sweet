<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY oper "http://sweet.jpl.nasa.gov/2.0/mathOperation.owl">
  <!ENTITY phys "http://sweet.jpl.nasa.gov/2.0/phys.owl">  
  <!ENTITY rate "http://sweet.jpl.nasa.gov/2.0/timeScale.owl">  
  <!ENTITY obj "http://sweet.jpl.nasa.gov/2.0/spaceObject.owl">
  <!ENTITY vect "http://sweet.jpl.nasa.gov/2.0/mathVector.owl">  
  <!ENTITY pres "http://sweet.jpl.nasa.gov/2.0/physPressure.owl">  
  <!ENTITY inst "http://sweet.jpl.nasa.gov/2.0/sciInstrument.owl">  
  <!ENTITY res "http://sweet.jpl.nasa.gov/2.0/sciResearch.owl">  
  <!ENTITY units "http://sweet.jpl.nasa.gov/2.0/sciUnits.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&pres;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:obj="&obj;"
         xmlns:oper="&oper;"
         xmlns:phys="&phys;"
         xmlns:rate="&rate;"
         xmlns:vect="&vect;"
         xmlns:pres="&pres;"
         xmlns:inst="&inst;"
         xmlns:units="&units;"
>
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.0">
     <owl:imports rdf:resource="&oper;"/>
     <owl:imports rdf:resource="&obj;"/>
     <owl:imports rdf:resource="&inst;"/>
     <owl:imports rdf:resource="&units;"/>
     <owl:imports rdf:resource="&phys;"/>
     <owl:imports rdf:resource="&rate;"/>
     <owl:imports rdf:resource="&vect;"/>
  </owl:Ontology>

<!-- Properties -->
  <owl:Class rdf:about="#Compressibility">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalProperty"/>
  </owl:Class>

  <owl:Class rdf:about="#Pressure">
    <rdfs:subClassOf rdf:resource="&phys;#PerAreaQuantity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#pascal"/>
        <owl:onProperty rdf:resource="&units;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&oper;#hasFirstOperand" />
        <owl:allValuesFrom rdf:resource="&phys;#Force" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Force per unit area.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#PartialPressure">
    <rdfs:subClassOf rdf:resource="#Pressure"/>
    <rdfs:comment xml:lang="en">The pressure that a component of a gaseous mixture would have if it alone occupied the same volume at the same temperature as the mixture.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#PressureGradient">
    <rdfs:subClassOf rdf:resource="&vect;#Gradient"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&oper;#hasFirstOperand" />
        <owl:allValuesFrom rdf:resource="#Pressure" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">The derivative of pressure.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Stress">
    <rdfs:subClassOf rdf:resource="#Pressure"/>
  </owl:Class>

  <owl:Class rdf:about="#TotalPressure">
    <rdfs:subClassOf rdf:resource="#Pressure"/>
  </owl:Class>

<!-- Phenomena -->
  <owl:Class rdf:about="#Depression">
    <rdfs:subClassOf rdf:resource="#LowPressure" />
  </owl:Class>

  <owl:Class rdf:about="#HighPressure">
    <rdfs:subClassOf rdf:resource="#Pressure" />
    <rdfs:label>High Pressure</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="#LowPressure">
    <rdfs:subClassOf rdf:resource="#Pressure" />
    <rdfs:label>Low Pressure</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="#PressureTendency">
    <rdfs:subClassOf rdf:resource="&rate;#Tendency"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&phys;#hasPhysicalProperty" />
        <owl:allValuesFrom rdf:resource="#Pressure" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">The character and amount of atmospheric pressure change during a specified period of time, often a three-hour period preceding an observation.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Ridge">
    <owl:equivalentClass rdf:resource="#HighPressure" />
    <rdfs:subClassOf rdf:resource="&obj;#Ridge" />
    <owl:disjointWith rdf:resource="#LowPressure" />
  </owl:Class>

  <owl:Class rdf:about="#Trough">
    <owl:equivalentClass rdf:resource="#LowPressure" />
    <rdfs:subClassOf rdf:resource="&obj;#Trough" />
    <rdfs:comment xml:lang="en">In meteorology, an elongated area of relatively low atmospheric pressure; the opposite of a ridge.</rdfs:comment>
  </owl:Class>

<!-- Processes -->
  <owl:Class rdf:about="#Compress">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalProcess"/>
    <owl:equivalentClass rdf:resource="#Compression"/>
  </owl:Class>
  <owl:Class rdf:about="#Compression"/>

  <owl:Class rdf:about="#Rareify">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalProcess"/>
    <owl:disjointWith rdf:resource="#Compress"/>
    <owl:equivalentClass rdf:resource="#Rareifaction"/>
    <owl:equivalentClass rdf:resource="#Decompress"/>
  </owl:Class>
  <owl:Class rdf:about="#Rareifaction"/>
  <owl:Class rdf:about="#Decompress"/>

<!-- States -->
  <owl:Class rdf:about="#Compressed">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalState"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess" />
        <owl:allValuesFrom rdf:resource="#Compress" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Pressurized">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalState"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess" />
        <owl:allValuesFrom rdf:resource="#Pressure" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Unpressurized">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalState"/>
    <owl:disjointWith rdf:resource="#Pressurized"/>
  </owl:Class>

  <owl:Class rdf:about="#Rareified">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalState"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess" />
        <owl:allValuesFrom rdf:resource="#Rareify" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

<!-- Instruments -->
  <owl:Class rdf:about="#Manometer">
    <rdfs:subClassOf rdf:resource="&inst;#Instrument"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&inst;#measures" />
        <owl:allValuesFrom rdf:resource="#Pressure" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

</rdf:RDF>
