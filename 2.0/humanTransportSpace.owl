<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY hum "http://sweet.jpl.nasa.gov/2.0/human.owl">
  <!ENTITY role "http://sweet.jpl.nasa.gov/2.0/sciRole.owl">
  <!ENTITY trans "http://sweet.jpl.nasa.gov/2.0/humanTransport.owl">
  <!ENTITY body "http://sweet.jpl.nasa.gov/2.0/astroBody.owl">
  <!ENTITY chem "http://sweet.jpl.nasa.gov/2.0/chem.owl">
  <!ENTITY air "http://sweet.jpl.nasa.gov/2.0/humanTransportAir.owl">
  <!ENTITY space "http://sweet.jpl.nasa.gov/2.0/humanTransportSpace.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&space;"
         xmlns:hum="&hum;"
         xmlns:air="&air;"
         xmlns:role="&role;"
         xmlns:trans="&trans;"
         xmlns:space="&space;"
         xmlns:body="&body;"
         xmlns:chem="&chem;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.0">
     <owl:imports rdf:resource="&hum;"/>   
     <owl:imports rdf:resource="&air;"/>   
     <owl:imports rdf:resource="&role;"/>   
     <owl:imports rdf:resource="&body;"/>   
     <owl:imports rdf:resource="&chem;"/>   
     <owl:imports rdf:resource="&trans;"/>   
  </owl:Ontology>


<!-- Space Travel -->
  <owl:Class rdf:about="#Liftoff">
    <rdfs:subClassOf rdf:resource="&hum;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Ascent">
    <rdfs:subClassOf rdf:resource="&hum;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Descent">
    <rdfs:subClassOf rdf:resource="&hum;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Propellant">
    <rdfs:subClassOf rdf:resource="&role;#Role"/>
  </owl:Class>

  <owl:Class rdf:about="#SpaceProgram">
    <rdfs:subClassOf rdf:resource="&hum;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#SpaceTravel">
    <rdfs:subClassOf rdf:resource="#SpaceProgram"/>
  </owl:Class>

  <owl:Class rdf:about="#Rocket">
    <rdfs:subClassOf rdf:resource="#Spacecraft"/>
  </owl:Class>

  <owl:Class rdf:about="#Satellite">
    <rdfs:subClassOf rdf:resource="#Spacecraft"/>
  </owl:Class>

  <owl:Class rdf:about="#Spacecraft">
    <rdfs:subClassOf rdf:resource="&trans;#TransportationEquipment"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&body;#OuterSpace"/>
        <owl:onProperty rdf:resource="&chem;#hasMedium"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#SpaceShuttle">
    <rdfs:subClassOf rdf:resource="#Spacecraft"/>
  </owl:Class>

</rdf:RDF>
