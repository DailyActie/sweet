<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY struc "http://sweet.jpl.nasa.gov/2.0/humanStructure.owl">
  <!ENTITY comm "http://sweet.jpl.nasa.gov/2.0/humanCommerce.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&struc;"
         xmlns:comm="&comm;"
         xmlns:struc="&struc;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.0">
     <owl:imports rdf:resource="&comm;"/>   
  </owl:Ontology>

<!-- Classes -->
  <owl:Class rdf:about="#Base">
    <rdfs:subClassOf rdf:resource="#Facility"/>
  </owl:Class>

  <owl:Class rdf:about="#Building">
    <rdfs:subClassOf rdf:resource="#Structure"/>
  </owl:Class>

  <owl:Class rdf:about="#CulturalFeature">
    <rdfs:subClassOf rdf:resource="#Structure"/>
  </owl:Class>

  <owl:Class rdf:about="#Dome">
    <rdfs:subClassOf rdf:resource="#Facility"/>
  </owl:Class>

  <owl:Class rdf:about="#Facility">
    <rdfs:subClassOf rdf:resource="#Structure"/>
  </owl:Class>

  <owl:Class rdf:about="#Habitat">
    <rdfs:subClassOf rdf:resource="#Building"/>
  </owl:Class>

  <owl:Class rdf:about="#Platform">
    <rdfs:subClassOf rdf:resource="#Structure"/>
  </owl:Class>

  <owl:Class rdf:about="#Station">
    <rdfs:subClassOf rdf:resource="#Facility"/>
  </owl:Class>

  <owl:Class rdf:about="#Structure">
    <rdfs:subClassOf rdf:resource="&comm;#Infrastructure"/>
  </owl:Class>

</rdf:RDF>
