<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY comm "http://sweet.jpl.nasa.gov/2.0/humanCommerce.owl">
  <!ENTITY commu "http://sweet.jpl.nasa.gov/2.0/humanCommunications.owl">
  <!ENTITY struc "http://sweet.jpl.nasa.gov/2.0/humanStructure.owl">
  <!ENTITY human "http://sweet.jpl.nasa.gov/2.0/human.owl">
  <!ENTITY res "http://sweet.jpl.nasa.gov/2.0/sciResearch.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&commu;"
         xmlns:human="&human;"
         xmlns:struc="&struc;"
         xmlns:commu="&commu;"
         xmlns:comm="&comm;"
         xmlns:res="&res;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.0">
     <owl:imports rdf:resource="&human;"/>   
     <owl:imports rdf:resource="&comm;"/>   
     <owl:imports rdf:resource="&res;"/>   
     <owl:imports rdf:resource="&struc;"/>   
  </owl:Ontology>

  <owl:Class rdf:about="#Antenna">
    <rdfs:subClassOf rdf:resource="#CommunicationEquipment"/>
  </owl:Class>

  <owl:Class rdf:about="#Communication">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#CommunicationEquipment"/>
        <owl:onProperty rdf:resource="&comm;#hasInfrastructure"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass rdf:resource="#Communications"/>
  </owl:Class>
  <owl:Class rdf:about="#Communications"/>

  <owl:Class rdf:about="#CommunicationEquipment">
    <rdfs:subClassOf rdf:resource="&comm;#Equipment"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Communications"/>
        <owl:onProperty rdf:resource="&human;#hasActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#CommunicationFacility">
    <rdfs:subClassOf rdf:resource="&struc;#Facility"/>
  </owl:Class>

  <owl:Class rdf:about="#CommunicationMethod">
    <rdfs:subClassOf rdf:resource="#Communication"/>
  </owl:Class>

  <owl:Class rdf:about="#Distortion">
    <rdfs:subClassOf rdf:resource="#Signal"/>
  </owl:Class>

  <owl:Class rdf:about="#Internet">
    <rdfs:subClassOf rdf:resource="#CommunicationMethod"/>
  </owl:Class>

  <owl:Class rdf:about="#LandLine">
    <rdfs:subClassOf rdf:resource="#Phone"/>
  </owl:Class>

  <owl:Class rdf:about="#Noisy">
    <rdfs:subClassOf rdf:resource="#Reception"/>
  </owl:Class>

  <owl:Class rdf:about="#Phone">
    <rdfs:subClassOf rdf:resource="#CommunicationEquipment"/>
    <rdfs:subClassOf rdf:resource="#CommunicationMethod"/>
  </owl:Class>

  <owl:Class rdf:about="#MobilePhone">
    <rdfs:subClassOf rdf:resource="#Phone"/>
  </owl:Class>

  <owl:Class rdf:about="#Radio">
    <rdfs:subClassOf rdf:resource="#CommunicationEquipment"/>
    <rdfs:subClassOf rdf:resource="#CommunicationMethod"/>
  </owl:Class>

  <owl:Class rdf:about="#Receive">
    <rdfs:subClassOf rdf:resource="#Communication"/>
    <owl:equivalentClass rdf:resource="#Reception"/>
  </owl:Class>
  <owl:Class rdf:about="#Reception"/>

  <owl:Class rdf:about="#Signal">
    <rdfs:subClassOf rdf:resource="#Communication"/>
  </owl:Class>

  <owl:Class rdf:about="#SignalToNoiseRatio">
    <rdfs:subClassOf rdf:resource="&res;#Index"/>
  </owl:Class>

  <owl:Class rdf:about="#Transmit">
    <rdfs:subClassOf rdf:resource="#Communication"/>
    <owl:equivalentClass rdf:resource="#Transmission"/>
  </owl:Class>
  <owl:Class rdf:about="#Transmission"/>

  <owl:Class rdf:about="#Wireless">
    <rdfs:subClassOf rdf:resource="#CommunicationMethod"/>
  </owl:Class>

</rdf:RDF>
