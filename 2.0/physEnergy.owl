<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY oper "http://sweet.jpl.nasa.gov/2.0/mathOperation.owl">
  <!ENTITY phys "http://sweet.jpl.nasa.gov/2.0/phys.owl">  
  <!ENTITY ener "http://sweet.jpl.nasa.gov/2.0/physEnergy.owl">  
  <!ENTITY sys "http://sweet.jpl.nasa.gov/2.0/sciSystem.owl">  
  <!ENTITY units "http://sweet.jpl.nasa.gov/2.0/sciUnits.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&ener;"
         xmlns:oper="&oper;"
         xmlns:phys="&phys;"
         xmlns:ener="&ener;"
         xmlns:sys="&sys;"
         xmlns:units="&units;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
>

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.0">
     <owl:imports rdf:resource="&oper;"/>
     <owl:imports rdf:resource="&phys;"/>
     <owl:imports rdf:resource="&sys;"/>
     <owl:imports rdf:resource="&units;"/>
  </owl:Ontology>

<!-- Energy -->
  <owl:Class rdf:about="#Energy">
    <rdfs:subClassOf rdf:resource="&phys;#ExtensiveProperty"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#joule"/>
        <owl:onProperty rdf:resource="&units;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#EnergyBudget">
    <rdfs:subClassOf rdf:resource="&sys;#Budget"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&phys;#hasPhysicalProperty" />
        <owl:allValuesFrom rdf:resource="#Energy" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#EnergyDensity">
    <rdfs:subClassOf rdf:resource="&phys;#PerAreaQuantity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#joule_perMeterSquared"/>
        <owl:onProperty rdf:resource="&units;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&oper;#hasFirstOperand" />
        <owl:allValuesFrom rdf:resource="#Energy" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Energy per unit area.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#EnergyTransfer">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalTransfer"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Energy" />
        <owl:onProperty rdf:resource="&phys;#hasPhysicalProperty"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Entropy">
    <rdfs:subClassOf rdf:resource="#Energy"/>
  </owl:Class>

  <owl:Class rdf:about="#Hamiltonian">
    <rdfs:subClassOf rdf:resource="#Energy"/>
  </owl:Class>

  <owl:Class rdf:about="#InternalEnergy">
    <rdfs:subClassOf rdf:resource="#Energy"/>
  </owl:Class>

  <owl:Class rdf:about="#Lagrangian">
    <rdfs:subClassOf rdf:resource="#Energy"/>
  </owl:Class>

  <owl:Class rdf:about="#PotentialEnergy">
    <rdfs:subClassOf rdf:resource="#Energy"/>
  </owl:Class>

  <owl:Class rdf:about="#Power">
    <rdfs:subClassOf rdf:resource="&phys;#ExtensiveProperty"/>
    <rdfs:subClassOf rdf:resource="&phys;#PerTimeQuantity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&oper;#hasFirstOperand" />
        <owl:allValuesFrom rdf:resource="#Energy" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#watt"/>
        <owl:onProperty rdf:resource="&units;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#SpecificPotentialEnergy">
    <rdfs:subClassOf rdf:resource="&phys;#PerMassQuantity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&oper;#hasFirstOperand" />
        <owl:allValuesFrom rdf:resource="#PotentialEnergy" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#joule_perKilogram"/>
        <owl:onProperty rdf:resource="&units;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Work">
    <rdfs:subClassOf rdf:resource="#Energy" />
    <rdfs:comment xml:lang="en">A form of energy arising from the motion of a system against a force, existing only in the process of energy conversion.</rdfs:comment>
  </owl:Class>

</rdf:RDF>
