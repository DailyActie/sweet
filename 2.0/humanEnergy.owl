<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY ener "http://sweet.jpl.nasa.gov/2.0/physEnergy.owl"> 
  <!ENTITY ther "http://sweet.jpl.nasa.gov/2.0/physThermo.owl"> 
  <!ENTITY phys "http://sweet.jpl.nasa.gov/2.0/phys.owl"> 
  <!ENTITY comm "http://sweet.jpl.nasa.gov/2.0/humanCommerce.owl"> 
  <!ENTITY role "http://sweet.jpl.nasa.gov/2.0/sciRole.owl"> 
  <!ENTITY syst "http://sweet.jpl.nasa.gov/2.0/sciSystem.owl"> 
  <!ENTITY huma "http://sweet.jpl.nasa.gov/2.0/human.owl"> 
  <!ENTITY eener "http://sweet.jpl.nasa.gov/2.0/humanEnergy.owl"> 
  <!ENTITY water "http://sweet.jpl.nasa.gov/2.0/chemWater.owl"> 
  <!ENTITY sust "http://sweet.jpl.nasa.gov/2.0/envirSustainability.owl"> 
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">

]>

<rdf:RDF xml:base="&eener;"
         xmlns:ener="&ener;"
         xmlns:comm="&comm;"
         xmlns:ther="&ther;"
         xmlns:role="&role;"
         xmlns:huma="&huma;"
         xmlns:phys="&phys;"
         xmlns:eener="&eener;"
         xmlns:water="&water;"
         xmlns:syst="&syst;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
	 xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
     <owl:imports rdf:resource="&ener;"/>
     <owl:imports rdf:resource="&comm;"/>
     <owl:imports rdf:resource="&sust;"/>
     <owl:imports rdf:resource="&syst;"/>
     <owl:imports rdf:resource="&huma;"/>
     <owl:imports rdf:resource="&role;"/>
     <owl:imports rdf:resource="&ther;"/>
     <owl:imports rdf:resource="&phys;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#ActiveSolarEnergy">
    <rdfs:subClassOf rdf:resource="#SolarEnergy"/>
    <rdfs:subClassOf rdf:resource="#ThermalCollection"/>
 </owl:Class>

  <owl:Class rdf:about="#Battery">
    <rdfs:subClassOf rdf:resource="#EnergyStorage"/>
  </owl:Class>

  <owl:Class rdf:about="#Biofuel">
    <rdfs:subClassOf rdf:resource="#Fuel"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergySource" />
        <owl:allValuesFrom rdf:resource="#Biomass" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Biodiesel">
    <rdfs:subClassOf rdf:resource="#Biofuel"/>
  </owl:Class>

  <owl:Class rdf:about="#BinaryCycle">
    <rdfs:subClassOf rdf:resource="#GeothermalPowerPlant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergySource" />
        <owl:allValuesFrom rdf:resource="#HeatExchanger" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Biomass">
    <rdfs:subClassOf rdf:resource="#RenewableEnergySource"/>
  </owl:Class>

  <owl:Class rdf:about="#Biopower">
    <rdfs:subClassOf rdf:resource="#RenewableEnergySource"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergySource" />
        <owl:allValuesFrom rdf:resource="#Biomass" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergyProduct" />
        <owl:someValuesFrom rdf:resource="#Electricity" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#CompundParabolicConcentrator">
    <rdfs:subClassOf rdf:resource="#EnergyCollector"/>
  </owl:Class>

  <owl:Class rdf:about="#ConcentratingSolarPower">
    <rdfs:subClassOf rdf:resource="#SolarEnergy"/>
  </owl:Class>

  <owl:Class rdf:about="#DirectUse">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalProcess"/>
    <owl:disjointWith rdf:resource="#EnergyConversionProcess"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergyProduct" />
        <owl:allValuesFrom rdf:resource="&ther;#Heat" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#DrySteam">
    <rdfs:subClassOf rdf:resource="#GeothermalPowerPlant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergySource" />
        <owl:allValuesFrom rdf:resource="&water;#Steam" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Electricity">
    <rdfs:subClassOf rdf:resource="#EnergyForm"/>
    <owl:equivalentClass rdf:resource="#PowerSource"/>
  </owl:Class>
  <owl:Class rdf:about="#PowerSource"/>

  <owl:Class rdf:about="#ElectricityProduction">
    <rdfs:subClassOf rdf:resource="#EnergyConversionProcess"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergyProduct" />
        <owl:allValuesFrom rdf:resource="#Electricity" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#EnergyCollection">
    <rdfs:subClassOf rdf:resource="#EnergyForm"/>
  </owl:Class>

  <owl:Class rdf:about="#EnergyCollector">
    <rdfs:subClassOf rdf:resource="#EnergyCollection"/>
  </owl:Class>

  <owl:Class rdf:about="#EnergyConservation">
    <rdfs:subClassOf rdf:resource="&sust;#Conservation"/>
  </owl:Class>

  <owl:Class rdf:about="#EnergyConversionDevice">
    <rdfs:subClassOf rdf:resource="&comm;#Infrastructure"/>
    <rdfs:subClassOf rdf:resource="#EnergySource"/>
  </owl:Class>

  <owl:Class rdf:about="#EnergyConversionProcess">
    <rdfs:subClassOf rdf:resource="&phys;#PhysicalProcess"/>
  </owl:Class>

  <owl:Class rdf:about="#EnergyDistributionSystem">
    <rdfs:subClassOf rdf:resource="&comm;#Infrastructure"/>
  </owl:Class>

  <owl:Class rdf:about="#EnergyEndUse">
    <rdfs:subClassOf rdf:resource="&ener;#Energy"/>
  </owl:Class>

  <owl:Class rdf:about="#EnergyForm">
    <rdfs:subClassOf rdf:resource="&ener;#Energy"/>
  </owl:Class>

  <owl:Class rdf:about="#EnergySource">
    <rdfs:subClassOf rdf:resource="&ener;#Energy"/>
  </owl:Class>

  <owl:Class rdf:about="#EnergyStorage">
    <rdfs:subClassOf rdf:resource="#EnergySource"/>
    <rdfs:subClassOf rdf:resource="&syst;#Storage"/>
  </owl:Class>

  <owl:Class rdf:about="#Ethanol">
    <rdfs:subClassOf rdf:resource="#Biofuel"/>
  </owl:Class>

  <owl:Class rdf:about="#FlashSteam">
    <rdfs:subClassOf rdf:resource="#GeothermalPowerPlant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergySource" />
        <owl:allValuesFrom rdf:resource="&water;#LiquidWater" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#FlatPlateCollector">
    <rdfs:subClassOf rdf:resource="#EnergyCollector"/>
  </owl:Class>

  <owl:Class rdf:about="#Fuel">
    <rdfs:subClassOf rdf:resource="#EnergyForm"/>
  </owl:Class>

  <owl:Class rdf:about="#FuelCell">
    <rdfs:subClassOf rdf:resource="#EnergyStorage"/>
  </owl:Class>

  <owl:Class rdf:about="#Gasification">
    <rdfs:subClassOf rdf:resource="#EnergyConversionProcess"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergyProduct" />
        <owl:allValuesFrom rdf:resource="#Fuel" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Geothermal">
    <rdfs:subClassOf rdf:resource="#RenewableEnergySource"/>
  </owl:Class>

  <owl:Class rdf:about="#GeothermalPowerPlant">
    <rdfs:subClassOf rdf:resource="#PowerPlant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasEnergySource" />
        <owl:allValuesFrom rdf:resource="#Geothermal" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Heater">
    <rdfs:subClassOf rdf:resource="#EnergyConversionDevice"/>
  </owl:Class>

  <owl:Class rdf:about="#HeatExchanger">
    <rdfs:subClassOf rdf:resource="#EnergyDistributionSystem"/>
  </owl:Class>

  <owl:Class rdf:about="#HeatPump">
    <rdfs:subClassOf rdf:resource="#EnergyDistributionSystem"/>
  </owl:Class>

  <owl:Class rdf:about="#HydrogenEnergy">
    <rdfs:subClassOf rdf:resource="#EnergyStorage"/>
  </owl:Class>

  <owl:Class rdf:about="#ParabolicTrough">
    <rdfs:subClassOf rdf:resource="#EnergyCollector"/>
  </owl:Class>

  <owl:Class rdf:about="#PassiveSolarEnergy">
    <rdfs:subClassOf rdf:resource="#SolarEnergy"/>
  </owl:Class>

  <owl:Class rdf:about="#Photovoltaics">
    <rdfs:subClassOf rdf:resource="#SolarPower"/>
  </owl:Class>

  <owl:Class rdf:about="#PowerGrid">
    <rdfs:subClassOf rdf:resource="#EnergyDistributionSystem"/>
  </owl:Class>

  <owl:Class rdf:about="#PowerPlant">
    <rdfs:subClassOf rdf:resource="#EnergyConversionDevice"/>
  </owl:Class>

  <owl:Class rdf:about="#PowerTransmissionFacility">
    <rdfs:subClassOf rdf:resource="&comm;#Infrastructure"/>
  </owl:Class>

  <owl:Class rdf:about="#Refrigerater">
    <rdfs:subClassOf rdf:resource="#EnergyConversionDevice"/>
  </owl:Class>

  <owl:Class rdf:about="#RenewableEnergySource">
    <rdfs:subClassOf rdf:resource="#EnergySource"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&role;#hasRole" />
        <owl:allValuesFrom rdf:resource="&sust;#Sustainability" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#SmartGrid">
    <rdfs:subClassOf rdf:resource="#PowerGrid"/>
  </owl:Class>

  <owl:Class rdf:about="#SolarEnergy">
    <rdfs:subClassOf rdf:resource="#RenewableEnergySource"/>
  </owl:Class>

  <owl:Class rdf:about="#SolarPower">
    <rdfs:subClassOf rdf:resource="#SolarEnergy"/>
    <rdfs:subClassOf rdf:resource="#PowerSource"/>
  </owl:Class>

  <owl:Class rdf:about="#SpaceCooling">
    <rdfs:subClassOf rdf:resource="#EnergyEndUse"/>
  </owl:Class>

  <owl:Class rdf:about="#SpaceHeating">
    <rdfs:subClassOf rdf:resource="#EnergyEndUse"/>
  </owl:Class>

  <owl:Class rdf:about="#SolarThermalPower">
    <rdfs:subClassOf rdf:resource="#SolarPower"/>
    <rdfs:subClassOf rdf:resource="#ThermalCollection"/>
  </owl:Class>

  <owl:Class rdf:about="#ThermalCollection">
    <rdfs:subClassOf rdf:resource="#EnergyCollector"/>
  </owl:Class>

  <owl:Class rdf:about="#TubularCollector">
    <rdfs:subClassOf rdf:resource="#EnergyCollector"/>
  </owl:Class>

  <owl:Class rdf:about="#WaterHeating">
    <rdfs:subClassOf rdf:resource="#EnergyEndUse"/>
  </owl:Class>

  <owl:Class rdf:about="#WindEnergy">
    <rdfs:subClassOf rdf:resource="#RenewableEnergySource"/>
    <rdfs:subClassOf rdf:resource="#PowerSource"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="#hasEnergySource">
    <rdfs:range rdf:resource="#EnergySource"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasEnergyProduct">
    <rdfs:range rdf:resource="#EnergyForm"/>
  </owl:ObjectProperty>

</rdf:RDF>
