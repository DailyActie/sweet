<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY sed "http://sweet.jpl.nasa.gov/2.0/landSediment.owl">
  <!ENTITY planet "http://sweet.jpl.nasa.gov/2.0/astroPlanet.owl">
  <!ENTITY geomor "http://sweet.jpl.nasa.gov/2.0/landGeomorphology.owl">
  <!ENTITY radm "http://sweet.jpl.nasa.gov/2.0/physRadiationMedium.owl">
  <!ENTITY dist "http://sweet.jpl.nasa.gov/2.0/spaceDistribution.owl">
  <!ENTITY xten "http://sweet.jpl.nasa.gov/2.0/spaceExtent.owl">
  <!ENTITY space "http://sweet.jpl.nasa.gov/2.0/space.owl">
  <!ENTITY chem "http://sweet.jpl.nasa.gov/2.0/chem.owl">
  <!ENTITY obj "http://sweet.jpl.nasa.gov/2.0/spaceObject.owl">
  <!ENTITY phys "http://sweet.jpl.nasa.gov/2.0/phys.owl">
  <!ENTITY land "http://sweet.jpl.nasa.gov/2.0/landLandform.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&land;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:obj="&obj;"
         xmlns:chem="&chem;"
         xmlns:xten="&xten;"
         xmlns:phys="&phys;"
         xmlns:radm="&radm;"
         xmlns:planet="&planet;"
         xmlns:space="&space;"
         xmlns:geomor="&geomor;"
         xmlns:dist="&dist;"
         xmlns:sed="&sed;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
     <owl:imports rdf:resource="&chem;"/>
     <owl:imports rdf:resource="&obj;"/>
     <owl:imports rdf:resource="&phys;"/>
     <owl:imports rdf:resource="&xten;"/>
     <owl:imports rdf:resource="&planet;"/>
     <owl:imports rdf:resource="&geomor;"/>
     <owl:imports rdf:resource="&space;"/>
     <owl:imports rdf:resource="&dist;"/>
     <owl:imports rdf:resource="&radm;"/>
     <owl:imports rdf:resource="&sed;"/>
  </owl:Ontology>

<!-- Realms -->
  <owl:Class rdf:about="#Continent">
    <rdfs:subClassOf rdf:resource="#LandRegion"/>
    <rdfs:comment xml:lang="en">A continent is one of several large landmasses. [Wikipedia]</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#DebrisBasin">
    <rdfs:subClassOf rdf:resource="#LandRegion"/>
  </owl:Class>

  <owl:Class rdf:about="#DeflationZone">
    <rdfs:subClassOf rdf:resource="#LandRegion"/>
  </owl:Class>

  <owl:Class rdf:about="#DryLand">
    <rdfs:subClassOf rdf:resource="#LandRegion"/>
  </owl:Class>

  <owl:Class rdf:about="#Land">
    <rdfs:subClassOf rdf:resource="&space;#Region"/>
    <owl:equivalentClass rdf:resource="#LandRegion"/>
    <owl:equivalentClass rdf:resource="#SurfaceRegion"/>
  </owl:Class>
  <owl:Class rdf:about="#SurfaceRegion"/>
  <owl:Class rdf:about="#LandRegion"/>

  <owl:Class rdf:about="#LandCover">
    <rdfs:subClassOf rdf:resource="#Landform"/>
    <rdfs:comment xml:lang="en">Land cover is the physical material at the surface of the earth. Land covers include grass, asphalt, trees, bare ground, water, etc. [Wikipedia]</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Landscape">
    <rdfs:subClassOf rdf:resource="#Landform"/>
    <rdfs:comment xml:lang="en">A landscape comprises the visible features of an area of land, including physical elements such as landforms, living elements of flora and fauna, abstract elements such as lighting and weather conditions, and human elements, for instance human activity or the built environment. Landscape may also signify the objects around one in a building. </rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Landform">
    <rdfs:subClassOf rdf:resource="#LandRegion"/>
    <rdfs:comment xml:lang="en">A landform comprises a geomorphological unit, and is largely defined by its surface form and location in the landscape, as part of the terrain, and as such, is typically an element of topography. Landforms are categorised by features such as elevation, slope, orientation, stratification, rock exposure, and soil type. They include berms, mounds, hills, cliffs, valleys, rivers and numerous other elements.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Lowland">
    <rdfs:subClassOf rdf:resource="#Landform"/>
    <owl:disjointWith rdf:resource="#Highland"/>
  </owl:Class>

  <owl:Class rdf:about="#Highland">
    <rdfs:subClassOf rdf:resource="#Landform"/>
  </owl:Class>

  <owl:Class rdf:about="#Mare">
    <rdfs:subClassOf rdf:resource="#Plain"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&radm;#hasVisibility" />
        <owl:allValuesFrom rdf:resource="&radm;#Dark" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Plain">
    <rdfs:subClassOf rdf:resource="#Landform"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&dist;#hasSpatialDistribution" />
        <owl:allValuesFrom rdf:resource="&dist;#Flat" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Prarie">
    <rdfs:subClassOf rdf:resource="#Plain"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="#hasGeomorphologicalType">
  </owl:ObjectProperty>

<!-- Aeolian -->
 <owl:Class rdf:about="#Desert">
    <rdfs:subClassOf rdf:resource="#Landform"/>
    <rdfs:comment xml:lang="en">A desert is a landscape form or region that receives very little precipitation.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Dune">
    <rdfs:subClassOf rdf:resource="#AeolianLandform"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&chem;#hasSubstance" />
        <owl:allValuesFrom rdf:resource="&sed;#Sand" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass rdf:resource="#SandDune"/>
   <rdfs:comment xml:lang="en">A dune is a hill of sand built by eolian processes. Dunes are subject to different forms and sizes based on their interaction with the wind. Most kinds of dune are longer on the windward side where the sand is pushed up the dune, and a shorter slip face in the lee of the wind. The valley or trough between dunes is called a slack. A dune field is an area covered by extensive sand dunes. Large dune fields are known as ergs </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#SandDune"/>

 <owl:Class rdf:about="#HighDesert">
    <rdfs:subClassOf rdf:resource="#Desert"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&phys;#hasPhysicalProperty" />
        <owl:allValuesFrom rdf:resource="&xten;#Elevation" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Aeolian">
    <rdfs:subClassOf rdf:resource="&geomor;#Weathering"/>
    <owl:equivalentClass rdf:resource="#Eolian"/>
    <rdfs:comment xml:lang="en">Aeolian (or Eolian or Ã†olian) processes pertain to the activity of the winds and more specifically, to the winds' ability to shape the surface of the Earth and other planets. Winds may erode, transport, and deposit materials, and are effective agents in regions with sparse vegetation and a large supply of unconsolidated sediments. [Wikipedia]</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Eolian"/>

  <owl:Class rdf:about="#AeolianLandform">
    <rdfs:subClassOf rdf:resource="#Landform"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&phys;#hasPhysicalProcess" />
        <owl:allValuesFrom rdf:resource="#Eolian" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Yardang">
    <rdfs:subClassOf rdf:resource="#AeolianLandform"/>
  </owl:Class>

<!-- Craters -->
  <owl:Class rdf:about="#CentralCreekCrater">
    <rdfs:subClassOf rdf:resource="#Crater"/>
  </owl:Class>

  <owl:Class rdf:about="#ComplexCrater">
    <rdfs:subClassOf rdf:resource="#Crater"/>
  </owl:Class>

  <owl:Class rdf:about="#Crater">
    <rdfs:subClassOf rdf:resource="#Landform"/>
    <rdfs:subClassOf rdf:resource="&obj;#Cavity"/>
  </owl:Class>

  <owl:Class rdf:about="#ImpactBasin">
    <rdfs:subClassOf rdf:resource="#Landform"/>
  </owl:Class>

  <owl:Class rdf:about="#ImpactCrater">
    <rdfs:subClassOf rdf:resource="#Crater"/>
  </owl:Class>

  <owl:Class rdf:about="#PedestalCrater">
    <rdfs:subClassOf rdf:resource="#Crater"/>
  </owl:Class>

  <owl:Class rdf:about="#PitCrater">
    <rdfs:subClassOf rdf:resource="#Crater"/>
  </owl:Class>

  <owl:Class rdf:about="#RockyPitFloorCrater">
    <rdfs:subClassOf rdf:resource="#PitCrater"/>
  </owl:Class>

  <owl:Class rdf:about="#ShatterCone">
    <rdfs:subClassOf rdf:resource="#Landform"/>
  </owl:Class>

  <owl:Class rdf:about="#Spall">
    <rdfs:subClassOf rdf:resource="#Landform"/>
  </owl:Class>

</rdf:RDF>
