      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&elem;#C"/>
        <owl:onProperty rdf:resource="&chem;#hasChemical_1"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&allo;#Cl2"/>
