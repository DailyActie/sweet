  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&cfc;"
         xmlns:owl="&owl;"
         xmlns:chem="&chem;"
         xmlns:comp="&comp;"
         xmlns:elem="&elem;"
         xmlns:allo="&allo;"
         xmlns:org="&org;"
         xmlns:cfc="&cfc;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.0">
     <owl:imports rdf:resource="&chem;"/>
     <owl:imports rdf:resource="&org;"/>
     <owl:imports rdf:resource="&elem;"/>
     <owl:imports rdf:resource="&allo;"/>
     <owl:imports rdf:resource="&comp;"/>
  </owl:Ontology>

<!-- CFC -->
  <owl:Class rdf:about="#CCl2F2">
    <rdfs:subClassOf rdf:resource="#CFC"/>
