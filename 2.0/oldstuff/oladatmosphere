  </owl:Class>

  <owl:Class rdf:about="#Frost">
    <rdfs:subClassOf rdf:resource="#WeatherPhenomena"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&process;Freezing"/>
        <owl:onProperty rdf:resource="&process;#hasProcess"/>
      </owl:Restriction>
    </rdfs:subClassOf>
