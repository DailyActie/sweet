  </owl:Class>

  <owl:Class rdf:about="#WarmSector">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasMeteorologicalPhenomena" />
        <owl:allValuesFrom rdf:resource="#Front" />
      </owl:Restriction>
