      <owl:Restriction>
        <owl:onProperty rdf:resource="&process;hasProcess" />
        <owl:allValuesFrom rdf:resource="&process;#Heating" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">In mountain meteorology, an upslope wind driven by heating (usually daytime insolation) at the slope surface under fair-weather conditions.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Anticyclone">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasMeteorologicalPhenomena" />
        <owl:allValuesFrom rdf:resource="#Wind" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&property;#hasPhysicalProperty" />
        <owl:allValuesFrom rdf:resource="&property;#Vorticity" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">An atmospheric anticyclonic circulation, a closed circulation. The wind in an anticyclone is in the clockwise direction in the Northern Hemisphere and counterclockwise in the Southern Hemisphere.</rdfs:comment>
  </owl:Class>

