      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Cloud" />
    <rdfs:comment xml:lang="en">A low-level, horizontal, wedge-shaped arcus cloud associated with a convective storm's gust front.</rdfs:comment>
  </owl:Class>

