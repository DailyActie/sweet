  <owl:Class rdf:about="#Altostratus">
    <rdfs:subClassOf rdf:resource="#Stratiform"/>
    <rdfs:comment xml:lang="en">A principal cloud type (cloud genus) in the form of a gray or bluish (never white) sheet or layer of striated, fibrous, or uniform appearance.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#AmbientTemperature">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&earthrealm;#hasEarthrealm" />
        <owl:allValuesFrom rdf:resource="&earthrealm;Atmosphere"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Temperature"/>
    <rdfs:label>Ambient Temperature</rdfs:label>
    <rdfs:comment>The temperature that is characteristic of the atmosphere surrounding a small-scale feature such as a cumulus cloud.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Anabatic_Wind">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&earthrealm;#hasEarthrealm" />
