    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">A narrow stream of air that transports large amounts of heat, moisture, and westerly momentum</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#WarmFront">
    <rdfs:subClassOf rdf:resource="#Front" />
    <owl:disjointWith rdf:resource="#ColdFront" />
