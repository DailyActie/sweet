<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [

  <!ENTITY calc "http://sweet.jpl.nasa.gov/2.0/mathCalculus.owl">
  <!ENTITY sci "http://sweet.jpl.nasa.gov/2.0/sci.owl">
  <!ENTITY math "http://sweet.jpl.nasa.gov/2.0/math.owl">
  <!ENTITY math2 "http://sweet.jpl.nasa.gov/2.0/math.owl#">
  <!ENTITY time "http://sweet.jpl.nasa.gov/2.0/time.owl">
  <!ENTITY time2 "http://sweet.jpl.nasa.gov/2.0/time.owl#">
  <!ENTITY units "http://sweet.jpl.nasa.gov/2.0/sciUnits.owl">
  <!ENTITY units2 "http://sweet.jpl.nasa.gov/2.0/sciUnits.owl#">

  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&time;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:sci="&sci;"
         xmlns:time2="&time2;"
         xmlns:math2="&math2;"
         xmlns:math="&math;"
         xmlns:calc="&calc;"
         xmlns:units2="&units2;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
     <owl:imports rdf:resource="&math;"/>
     <owl:imports rdf:resource="&calc;"/>
     <owl:imports rdf:resource="&sci;"/>
     <owl:imports rdf:resource="&units;"/>
  </owl:Ontology>

<!-- Classes -->
  <owl:Class rdf:about="#Age"/>

  <owl:Class rdf:about="#Autumn">
    <owl:equivalentClass rdf:resource="#Fall"/>
  </owl:Class>

  <owl:Class rdf:about="#Beginning">
    <owl:equivalentClass rdf:resource="#Start"/>
  </owl:Class>

  <owl:Class rdf:about="#Century">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#CurrentTime">
    <rdfs:subClassOf rdf:resource="#Instant"/>
  </owl:Class>

  <owl:Class rdf:about="#Day">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Daytime">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Decade">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Delay">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Duration">
    <rdfs:subClassOf rdf:resource="#TemporalExtent"/>
    <rdfs:subClassOf rdf:resource="&math;#Interval"/>
  </owl:Class>

  <owl:Class rdf:about="#End">
    <rdfs:subClassOf rdf:resource="#TemporalSubset"/>
  </owl:Class>

  <owl:Class rdf:about="#Evening">
    <rdfs:subClassOf rdf:resource="#Nighttime"/>
  </owl:Class>

  <owl:Class rdf:about="#Event">
    <owl:equivalentClass rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Fall">
    <rdfs:subClassOf rdf:resource="#Season"/>
  </owl:Class>

  <owl:Class rdf:about="#History">
    <rdfs:subClassOf rdf:resource="#Period"/>
  </owl:Class>

  <owl:Class rdf:about="#Hour">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Instant">
    <rdfs:subClassOf rdf:resource="#TemporalExtent"/>
    <rdfs:subClassOf rdf:resource="&math;#Point"/>
  </owl:Class>

  <owl:Class rdf:about="#Instantaneous">
    <owl:equivalentClass rdf:resource="#Instant"/>
  </owl:Class>

  <owl:Class rdf:about="#LocalTime">
  </owl:Class>

  <owl:Class rdf:about="#Millennium">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Minute">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Month">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Morning">
    <rdfs:subClassOf rdf:resource="#Daytime"/>
  </owl:Class>


  <owl:Class rdf:about="#NeotectonicPeriod">
    <rdfs:subClassOf rdf:resource="#Period"/>
  </owl:Class>

  <owl:Class rdf:about="#Nighttime">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Old">
    <rdfs:subClassOf rdf:resource="#Age"/>
  </owl:Class>

  <owl:Class rdf:about="#Occurrence">
    <owl:equivalentClass rdf:resource="#Event"/>
  </owl:Class>

  <owl:Class rdf:about="#Period">
    <rdfs:subClassOf rdf:resource="#TemporalExtent"/>
  </owl:Class>

  <owl:Class rdf:about="#Prehistoric">
    <rdfs:subClassOf rdf:resource="#Period"/>
  </owl:Class>


  <owl:Class rdf:about="#Rate">
    <rdfs:subClassOf rdf:resource="&calc;#PartialDerivative"/>
  </owl:Class>

  <owl:Class rdf:about="#Season">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Second">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#SolarTime">
  </owl:Class>

  <owl:Class rdf:about="#Spring">
    <rdfs:subClassOf rdf:resource="#Season"/>
  </owl:Class>

  <owl:Class rdf:about="#Start">
    <rdfs:subClassOf rdf:resource="#TemporalSubset"/>
  </owl:Class>

  <owl:Class rdf:about="#Summer">
    <rdfs:subClassOf rdf:resource="#Season"/>
  </owl:Class>

  <owl:Class rdf:about="#TemporalEntity"/>

  <owl:Class rdf:about="#TemporalExtent">
    <rdfs:subClassOf rdf:resource="#TemporalEntity"/>
  </owl:Class>

  <owl:Class rdf:about="#TemporalReference"/>

  <owl:Class rdf:about="#TemporalSubset">
    <rdfs:subClassOf rdf:resource="#TemporalEntity"/>
    <rdfs:subClassOf rdf:resource="&math;#Subset"/>
  </owl:Class>

  <owl:Class rdf:about="#TemporalScale">
    <rdfs:subClassOf rdf:resource="&sci;#Scale" />
  </owl:Class>

  <owl:Class rdf:about="#Tendency">
    <owl:equivalentClass rdf:resource="#Rate"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&calc;#derivativeWithRespectTo"/>
        <owl:allValuesFrom rdf:resource="#Time"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#perSecond"/>
        <owl:onProperty rdf:resource="&units;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>


  <owl:Class rdf:about="#Time">
    <rdfs:subClassOf rdf:resource="&sci;#Quantity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#second"/>
        <owl:onProperty rdf:resource="&units;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#TimeInterval">
    <rdfs:subClassOf rdf:resource="#Duration"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Time"/>
        <owl:onProperty rdf:resource="&math;#hasUpperLimit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Time"/>
        <owl:onProperty rdf:resource="&math;#hasLowerLimit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#TimeStep">
  </owl:Class>

  <owl:Class rdf:about="#TimeZone">
    <rdfs:subClassOf rdf:resource="#TimeInterval"/>
  </owl:Class>

  <owl:Class rdf:about="#UT"/>
  <owl:Class rdf:about="#UniversalTime">
    <rdfs:subClassOf rdf:resource="#TemporalReference"/>
    <owl:equivalentClass rdf:resource="#UT"/>
  </owl:Class>

  <owl:Class rdf:about="#Week">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Winter">
    <rdfs:subClassOf rdf:resource="#Season"/>
  </owl:Class>

  <owl:Class rdf:about="#Year">
    <rdfs:subClassOf rdf:resource="#Duration"/>
  </owl:Class>

  <owl:Class rdf:about="#Young">
    <rdfs:subClassOf rdf:resource="#Age"/>
  </owl:Class>

<!-- Annotation Properties -->
  <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>

<!-- Datatype Properties -->
  <owl:DatatypeProperty rdf:about="#fractionOfYear">
    <rdf:type rdf:resource="&owl;FunctionalProperty"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#numberOfYears">
    <rdf:type rdf:resource="&owl;FunctionalProperty"/>
  </owl:DatatypeProperty>

<!-- Object Properties -->
  <owl:ObjectProperty rdf:about="#after">
    <rdfs:subPropertyOf rdf:resource="&math;#greaterThan"/>
    <owl:inverseOf rdf:resource="#before"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#before">
    <rdfs:subPropertyOf rdf:resource="&math;#lessThan"/>
    <owl:inverseOf rdf:resource="#after"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#followedBy">
    <rdfs:subPropertyOf rdf:resource="#after"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasDuration">
    <rdfs:range rdf:resource="#Duration"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasPeriod">
    <rdfs:range rdf:resource="#Period"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasRate">
    <rdfs:range rdf:resource="#Rate"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasTime">
    <rdfs:range rdf:resource="#Time"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasTemporalScale">
    <rdfs:range rdf:resource="#TemporalScale"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasTemporalExtent">
    <rdfs:range rdf:resource="#Time"/>
  </owl:ObjectProperty>

<!-- Individuals -->

  <time2:TemporalScale rdf:about="#Annual">
    <math2:hasLowerBound rdf:datatype="&xsd;double">0.75</math2:hasLowerBound>
    <math2:hasUpperBound rdf:datatype="&xsd;double">1.5</math2:hasUpperBound>
    <units2:hasUnit rdf:resource="&units;#year"/>
  </time2:TemporalScale>

  <time2:TemporalScale rdf:about="#Biannual">
    <math2:hasLowerBound rdf:datatype="&xsd;double">1.5</math2:hasLowerBound>
    <math2:hasUpperBound rdf:datatype="&xsd;double">2.5</math2:hasUpperBound>
    <units2:hasUnit rdf:resource="&units;#year"/>
  </time2:TemporalScale>

  <time2:TemporalScale rdf:about="#Decadal">
    <math2:hasLowerBound rdf:datatype="&xsd;double">10</math2:hasLowerBound>
    <math2:hasUpperBound rdf:datatype="&xsd;double">50</math2:hasUpperBound>
    <units2:hasUnit rdf:resource="&units;#year"/>
  </time2:TemporalScale>

</rdf:RDF>
