<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY res "http://sweet.jpl.nasa.gov/2.0/sciResearch.owl">
  <!ENTITY sys "http://sweet.jpl.nasa.gov/2.0/sciSystem.owl">
  <!ENTITY ordi "http://sweet.jpl.nasa.gov/2.0/sciOrdinal.owl">  
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.0/mathRelation.owl">  
  <!ENTITY role "http://sweet.jpl.nasa.gov/2.0/sciRole.owl">  
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&role;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:ordi="&ordi;"
         xmlns:role="&role;"
         xmlns:rela="&rela;"
         xmlns:res="&res;"
         xmlns:sys="&sys;"
>
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&ordi;"/>
     <owl:imports rdf:resource="&res;"/>
     <owl:imports rdf:resource="&sys;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#Catalyst">
    <rdfs:subClassOf rdf:resource="#Role"/>
  </owl:Class>

  <owl:Class rdf:about="#Dominant">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Subordinate"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Inhibitor">
    <rdfs:subClassOf rdf:resource="#Role"/>
  </owl:Class>

  <owl:Class rdf:about="#Main">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#Major">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Minor"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Minor">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#Negative">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
    <owl:disjointWith rdf:resource="#Positive"/>
  </owl:Class>

  <owl:Class rdf:about="#Positive">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#Primary">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Secondary"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Principal">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Secondary"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Role">
    <rdfs:subClassOf rdf:resource="&sys;#SystemComponent"/>
  </owl:Class>

  <owl:Class rdf:about="#RoleCategory">
    <rdfs:subClassOf rdf:resource="&ordi;#OrderCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Role"/>
        <owl:onProperty rdf:resource="&res;#measureOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Secondary">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#Subordinate">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#Trace">
    <rdfs:subClassOf rdf:resource="#RoleCategory"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="#hasRole">
    <rdfs:range rdf:resource="#Role"/>
  </owl:ObjectProperty>

</rdf:RDF>
