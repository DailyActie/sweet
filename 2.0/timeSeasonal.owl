<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [

  <!ENTITY root "http://sweet.jpl.nasa.gov/2.0/">
  <!ENTITY seas "&root;timeSeasonal.owl#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY owlt "http://www.w3.org/2006/time#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&seas;"
         xmlns:owl="&owl;"
         xmlns:owlt="&owlt;"
         xmlns:rdf="&rdf;"
         xmlns:seas="&seas;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
  </owl:Ontology>

  <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>

  <owl:Class rdf:about="#Autumn">
    <owl:equivalentClass rdf:resource="#Fall"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#AutumnalEquinox"/>
        <owl:onProperty rdf:resource="&owlt;hasBeginning"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#WinterSolstice"/>
        <owl:onProperty rdf:resource="&owlt;hasEnd"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#AutumnalEquinox">
    <rdfs:subClassOf rdf:resource="#Equinox"/>
    <owl:equivalentClass rdf:resource="#FallEquinox"/>
  </owl:Class>
  <owl:Class rdf:about="#FallEquinox"/>

  <owl:Class rdf:about="#Equinox">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
  </owl:Class>

  <owl:Class rdf:about="#Fall">
    <rdfs:subClassOf rdf:resource="#Season"/>
  </owl:Class>

  <owl:Class rdf:about="#Season">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
  </owl:Class>

  <owl:Class rdf:about="#Solstice">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
  </owl:Class>

  <owl:Class rdf:about="#Spring">
    <rdfs:subClassOf rdf:resource="#Season"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#SpringEquinox"/>
        <owl:onProperty rdf:resource="&owlt;hasBeginning"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#SummerSolstice"/>
        <owl:onProperty rdf:resource="&owlt;hasEnd"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#SpringEquinox">
    <rdfs:subClassOf rdf:resource="#Equinox"/>
  </owl:Class>

  <owl:Class rdf:about="#Summer">
    <rdfs:subClassOf rdf:resource="#Season"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#SummerSolstice"/>
        <owl:onProperty rdf:resource="&owlt;hasBeginning"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#FallEquinox"/>
        <owl:onProperty rdf:resource="&owlt;hasEnd"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#SummerSolstice">
    <rdfs:subClassOf rdf:resource="#Solstice"/>
  </owl:Class>

  <owl:Class rdf:about="#Winter">
    <rdfs:subClassOf rdf:resource="#Season"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#WinterSolstice"/>
        <owl:onProperty rdf:resource="&owlt;hasBeginning"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#SpringEquinox"/>
        <owl:onProperty rdf:resource="&owlt;hasEnd"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#WinterSolstice">
    <rdfs:subClassOf rdf:resource="#Solstice"/>
  </owl:Class>

</rdf:RDF>
