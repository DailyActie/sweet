  </oref:EarthOcean>
  <oref:OceanDepth rdf:about="#1038m">
     <math:hasNumericValue rdf:datatype="&xsd;double">1038</math:hasNumericValue>
  </oref:OceanDepth>
  <oref:OceanArea rdf:about="#14pt1million_km2">
     <math:hasNumericValue rdf:datatype="&xsd;double">14.1</math:hasNumericValue>
  </oref:OceanArea>

  <oref:EarthOcean rdf:about="#IndianOcean">
     <xten:hasAverageDepth rdf:resource="#3963m" />
     <xten:hasMaximumDepth rdf:resource="#8047m" />
     <xten:hasVolume rdf:resource="#291pt0million_km3" />
     <xten:hasArea rdf:resource="#73pt4million_km2" />
  </oref:EarthOcean>
  <oref:OceanDepth rdf:about="#3963m">
     <math:hasNumericValue rdf:datatype="&xsd;double">3963</math:hasNumericValue>
  </oref:OceanDepth>
  <oref:OceanDepth rdf:about="#8047m">
     <math:hasNumericValue rdf:datatype="&xsd;double">8047</math:hasNumericValue>
  </oref:OceanDepth>
  <oref:OceanVolume rdf:about="#291pt0million_km3">
     <math:hasNumericValue rdf:datatype="&xsd;double">291.0</math:hasNumericValue>
  </oref:OceanVolume>
  <oref:OceanArea rdf:about="#73pt4million_km2">
     <math:hasNumericValue rdf:datatype="&xsd;double">73.4</math:hasNumericValue>
  </oref:OceanArea>

  <oref:EarthOcean rdf:about="#CaribbeanSea">
     <xten:hasMaximumDepth rdf:resource="#7686m" />
     <xten:hasArea rdf:resource="#2pt8million_km2" />
  </oref:EarthOcean>
  <oref:OceanDepth rdf:about="#7686m">
     <math:hasNumericValue rdf:datatype="&xsd;double">7686</math:hasNumericValue>
