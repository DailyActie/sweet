     <xten:hasVolume rdf:resource="#323pt6million_km3" />
     <xten:hasArea rdf:resource="#82pt4million_km2" />
  </oref:EarthOcean>
  <oref:OceanDepth rdf:about="#3926m">
     <math:hasNumericValue rdf:datatype="&xsd;double">3926</math:hasNumericValue>
  </oref:OceanDepth>
  <oref:OceanDepth rdf:about="#8605m">
     <math:hasNumericValue rdf:datatype="&xsd;double">8605</math:hasNumericValue>
