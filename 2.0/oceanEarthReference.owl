<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY root   "http://sweet.jpl.nasa.gov/2.0/">
  <!ENTITY math   "&root;math.owl#">
  <!ENTITY ocean  "&root;ocean.owl#">
  <!ENTITY odyn   "&root;oceanCirculation.owl#">
  <!ENTITY oref   "&root;oceanEarthReference.owl#">
  <!ENTITY xten   "&root;spaceExtent.owl#">
  <!ENTITY units  "&root;sciUnits.owl#">
  <!ENTITY planet "&root;astroPlanet.owl#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&oref;"
         xmlns:owl="&owl;"
         xmlns:xten="&xten;"
         xmlns:odyn="&odyn;"
         xmlns:planet="&planet;"
         xmlns:math="&math;"
         xmlns:oref="&oref;"
         xmlns:ocean="&ocean;"
         xmlns:units="&units;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.0">
     <owl:imports rdf:resource="&root;math.owl"/>
     <owl:imports rdf:resource="&root;ocean.owl"/>
     <owl:imports rdf:resource="&root;oceanCirculation.owl"/>
     <owl:imports rdf:resource="&root;spaceExtent.owl"/>
     <owl:imports rdf:resource="&root;astroPlanet.owl"/>
     <owl:imports rdf:resource="&root;sciUnits.owl"/>
  </owl:Ontology>

<!-- Oceans -->
  <ocean:PhoticZone rdf:ID="EarthPhoticZone">
    <planet:hasReferenceDepth rdf:resource="#0to200m"/>
    <planet:hasPlanet rdf:resource="&planet;Earth"/>
  </ocean:PhoticZone>
  <xten:DepthRange rdf:about="#0to200m">
    <math:hasLowerBound rdf:datatype="&xsd;double">0</math:hasLowerBound>
    <math:hasUpperBound rdf:datatype="&xsd;double">200</math:hasUpperBound>
    <units:hasUnit rdf:resource="&units;meter"/>
  </xten:DepthRange>

  <ocean:AbyssopelagicZone rdf:ID="EarthAbyssopelagicZone">
    <planet:hasReferenceDepth rdf:resource="#4pluskm"/>
    <planet:hasPlanet rdf:resource="&planet;Earth"/>
  </ocean:AbyssopelagicZone>
  <xten:DepthRange rdf:about="#4pluskm">
    <math:hasLowerBound rdf:datatype="&xsd;double">4</math:hasLowerBound>
    <units:hasUnit rdf:resource="&units;kilometer"/>
  </xten:DepthRange>

  <ocean:BathypelagicZone rdf:ID="EarthBathypelagicZone">
    <planet:hasReferenceDepth rdf:resource="#1to4km"/>
    <planet:hasPlanet rdf:resource="&planet;Earth"/>
  </ocean:BathypelagicZone>
  <xten:DepthRange rdf:about="#1to4km">
    <math:hasLowerBound rdf:datatype="&xsd;double">1</math:hasLowerBound>
    <math:hasUpperBound rdf:datatype="&xsd;double">4</math:hasUpperBound>
    <units:hasUnit rdf:resource="&units;kilometer"/>
  </xten:DepthRange>

  <ocean:MesopelagicZone rdf:ID="EarthMesopelagicZone">
    <planet:hasReferenceDepth rdf:resource="#200to1000m"/>
    <planet:hasPlanet rdf:resource="&planet;Earth"/>
  </ocean:MesopelagicZone>
  <xten:DepthRange rdf:about="#200to1000m">
    <math:hasLowerBound rdf:datatype="&xsd;double">200</math:hasLowerBound>
    <math:hasUpperBound rdf:datatype="&xsd;double">1000</math:hasUpperBound>
    <units:hasUnit rdf:resource="&units;meter"/>
  </xten:DepthRange>

  <owl:Class rdf:about="#EarthOcean">
    <rdfs:subClassOf rdf:resource="&planet;Ocean" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&planet;hasPlanet" />
        <owl:hasValue rdf:resource="&planet;Earth" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#EarthOceanCurrent">
    <rdfs:subClassOf rdf:resource="&odyn;OceanCurrent" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&planet;hasPlanet" />
        <owl:hasValue rdf:resource="&planet;Earth" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#EarthWesternBoundaryCurrent">
    <rdfs:subClassOf rdf:resource="&odyn;WesternBoundaryCurrent" />
    <rdfs:subClassOf rdf:resource="#EarthOceanCurrent" />
  </owl:Class>

  <owl:Class rdf:about="#EarthEasternBoundaryCurrent">
    <rdfs:subClassOf rdf:resource="&odyn;EasternBoundaryCurrent" />
    <rdfs:subClassOf rdf:resource="#EarthOceanCurrent" />
  </owl:Class>

  <owl:Class rdf:about="#OceanDepth">
    <rdfs:subClassOf rdf:resource="&xten;Depth" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&units;hasUnit" />
        <owl:hasValue rdf:resource="&units;meter" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#OceanArea">
    <rdfs:subClassOf rdf:resource="&xten;Area" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&units;hasUnit" />
        <owl:hasValue rdf:resource="#million_km2" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#OceanVolume">
    <rdfs:subClassOf rdf:resource="&xten;Volume" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&units;hasUnit" />
        <owl:hasValue rdf:resource="#million_km3" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <units:UnitDerivedByScaling rdf:ID="million_km2">
    <units:hasScalingNumber rdf:datatype="&xsd;double">1e-12</units:hasScalingNumber>
    <units:derivedFromUnit rdf:resource="&units;meterSquared"/>
  </units:UnitDerivedByScaling>

  <units:UnitDerivedByScaling rdf:ID="million_km3">
    <units:hasScalingNumber rdf:datatype="&xsd;double">1e-15</units:hasScalingNumber>
    <units:derivedFromUnit rdf:resource="&units;meterCubed"/>
  </units:UnitDerivedByScaling>

  <oref:EarthOcean rdf:about="#PacificOcean">
