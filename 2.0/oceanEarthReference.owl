     <xten:hasMaximumDepth rdf:resource="#10911m" />
     <xten:hasVolume rdf:resource="#707pt6million_km3" />
     <xten:hasArea rdf:resource="#165pt2million_km2" />
  </oref:EarthOcean>
  <oref:OceanDepth rdf:about="#4282m">
     <math:hasNumericValue rdf:datatype="&xsd;double">4282</math:hasNumericValue>
  </oref:OceanDepth>
  <oref:OceanDepth rdf:about="#10911m">
     <math:hasNumericValue rdf:datatype="&xsd;double">10911</math:hasNumericValue>
