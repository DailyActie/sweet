@prefix dec: <http://sweetontology.net/humanDecision/> .
@prefix graph: <http://sweetontology.net/reprMathGraph/> .
@prefix hrela: <http://sweetontology.net/relaHuman/> .
@prefix human: <http://sweetontology.net/human/> .
@prefix oper: <http://sweetontology.net/reprMathOperation/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prop: <http://sweetontology.net/prop/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sweetontology.net/humanDecision>
  rdf:type owl:Ontology ;
  rdfs:label "SWEET Ontology" ;
  owl:imports <http://sweetontology.net/human> ;
  owl:imports <http://sweetontology.net/prop> ;
  owl:imports <http://sweetontology.net/relaHuman> ;
  owl:imports <http://sweetontology.net/reprMathGraph> ;
  owl:imports <http://sweetontology.net/reprMathOperation> ;
  owl:versionInfo "3.1.0" ;
.
dec:Allocation
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:Attribute
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:BeliefNetwork
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:Choice
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:ConsumerBehavior
  rdf:type owl:Class ;
  rdfs:subClassOf human:SocialBehavior ;
.
dec:ControlStrategy
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:Criteria
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:DSS
  rdf:type owl:Class ;
.
dec:Decision
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:DecisionActivity
  rdf:type owl:Class ;
  rdfs:subClassOf human:HumanActivity ;
.
dec:DecisionMaker
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
  rdfs:subClassOf prop:Role ;
.
dec:DecisionSupportSystem
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
  owl:equivalentClass dec:DSS ;
.
dec:DecisionTree
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
  rdfs:subClassOf graph:Tree ;
.
dec:EnvironmentalControlObjective
  rdf:type owl:Class ;
  rdfs:subClassOf dec:ControlStrategy ;
  rdfs:subClassOf dec:Objective ;
.
dec:Evaluation
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:GroupConsensus
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:LocationAllocation
  rdf:type owl:Class ;
  rdfs:comment "Spatial allocation is primarily concerned with designating what kinds of activities can or will be done where on the landscape. Land-use zoning is a typical example of a spatial allocation problem in which the landscape is divided up into a set of multiple alternative uses such as industrial, commercial, residential, etc. Allocation to a particular use usually depends on intrinsic properties of the individual parcels as well as adjacency constraints."@en ;
  rdfs:subClassOf dec:Allocation ;
.
dec:Multiattribute
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onProperty hrela:hasAttribute ;
    ] ;
.
dec:Multiobjective
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onProperty hrela:hasObjective ;
    ] ;
.
dec:MultipleDecisionMaker
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onProperty hrela:hasDecisionMaker ;
    ] ;
.
dec:Objective
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:Optimization
  rdf:type owl:Class ;
.
dec:Optimize
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
  rdfs:subClassOf oper:OperationOnFunction ;
  owl:equivalentClass dec:Optimization ;
.
dec:Preference
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:Ranking
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:ResourceAllocation
  rdf:type owl:Class ;
  rdfs:comment "Resource allocation has two meanings. One meaning refers to allocating a resource such as forest land to two or more designated uses. For example, forest land units could be allocated to timber production, recreation, etc. The second meaning is in the sense of allocating management resources. This second meaning is concerned with allocating time, materials, personnel, budget to landscape elements to accomplish meanegement objectives such as protection, restoration, timber production, etc."@en ;
  rdfs:subClassOf dec:Allocation ;
.
dec:Scenario
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:Screening
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:SolutionStrategy
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:Valuation
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
dec:Weighting
  rdf:type owl:Class ;
  rdfs:subClassOf dec:DecisionActivity ;
.
