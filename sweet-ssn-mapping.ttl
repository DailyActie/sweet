# baseURI: http:///sweetontology.net/alignment/ssn
# imports: http://sweetontology.net/humanResearch
# imports: http://sweetontology.net/prop
# imports: http://sweetontology.net/reprMathStatistics
# imports: http://www.w3.org/ns/ssn/
# prefix: ssn

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ssn: <http:///sweetontology.net/alignment/ssn#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http:///sweetontology.net/alignment/ssn>
  rdf:type owl:Ontology ;
  owl:imports <http://sweetontology.net/humanResearch> ;
  owl:imports <http://sweetontology.net/prop> ;
  owl:imports <http://sweetontology.net/reprMathStatistics> ;
  owl:imports <http://www.w3.org/ns/ssn/> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
<http://sweetontology.net/humanResearch#Observation>
  rdfs:subClassOf <http://www.w3.org/ns/sosa/Observation> ;
.
<http://sweetontology.net/humanResearch#Result>
  rdfs:subClassOf <http://www.w3.org/ns/sosa/Result> ;
.
<http://sweetontology.net/humanResearch#Sample>
  rdfs:subClassOf <http://www.w3.org/ns/sosa/Sampling> ;
.
<http://sweetontology.net/humanResearch#Variable>
  rdfs:comment "Not quite sure about alignment to ssn:Property here" ;
  rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
.
<http://sweetontology.net/prop#Property>
  owl:equivalentClass <http://www.w3.org/ns/ssn/Property> ;
.
<http://sweetontology.net/reprMathStatistics#Sampling>
  rdfs:subClassOf <http://www.w3.org/ns/sosa/Sampling> ;
.
<http://sweetontology.net/reprMathStatistics#StatisticalSample>
  rdfs:subClassOf <http://www.w3.org/ns/sosa/Sample> ;
  skos:note "Is the subclass relationship from StatisticalSample to humanResearchSample the right way round here? Or are they both subclasses of sosa:Sample? " ;
.
