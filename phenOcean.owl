<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
   <!ENTITY phen "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/phen.owl">
   <!ENTITY realm "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/realm.owl">
   <!ENTITY rela "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/rela.owl">
   <!ENTITY ocea "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/phenOcean.owl#">
   <!ENTITY owl "http://www.w3.org/2002/07/owl#">
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&ocea;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
	 xmlns:xsd = "&xsd;"
         xmlns:phen="&phen;"
         xmlns:rela="&rela;"
         xmlns:realm="&realm;"
         xmlns:ocea="&ocea;"
>

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.4">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&realm;"/>     
     <owl:imports rdf:resource="&rela;"/>     
     <owl:imports rdf:resource="&phen;"/>     
  </owl:Ontology>

  <owl:Class rdf:about="#OceanPhenomena">
    <rdfs:subClassOf rdf:resource="&phen;#PlanetaryPhenomena"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&realm;#Ocean"/>
        <owl:onProperty rdf:resource="&rela;#hasRealm"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

</rdf:RDF>
