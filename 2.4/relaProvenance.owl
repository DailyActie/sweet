<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY prrela "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/relaProvenance.owl#">
  <!ENTITY mrela "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/relaMath.owl#">
  <!ENTITY prov "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprSciProvenance.owl#">
  <!ENTITY comp "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprSciComponent.owl#">
  <!ENTITY tstate "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/stateThermodynamic.owl#">
  <!ENTITY velo "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propSpeed.owl#">
  <!ENTITY prop "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/prop.owl#">
  <!ENTITY proc "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/proc.owl#">
  <!ENTITY data "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprDataModel.owl#">
  <!ENTITY ordi "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propOrdinal.owl#">
  <!ENTITY serv "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprDataService.owl#">
  <!ENTITY prod "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprDataProduct.owl#">
  <!ENTITY quan "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propQuantity.owl#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&prrela;"
         xmlns:data="&data;"
         xmlns:prod="&prod;"
         xmlns:prrela="&prrela;"
         xmlns:mrela="&mrela;"
         xmlns:prop="&prop;"
         xmlns:proc="&proc;"
         xmlns:prov="&prov;"
         xmlns:comp="&comp;"
         xmlns:serv="&serv;"
         xmlns:quan="&quan;"
         xmlns:ordi="&ordi;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
>
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprSciProvenance.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprSciComponent.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/prop.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/proc.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprDataProduct.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprDataService.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprDataModel.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propOrdinal.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propQuantity.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/relaMath.owl"/>
  </owl:Ontology>

<!-- provenance -->
  <owl:ObjectProperty rdf:about="#hasSource">
    <rdfs:range rdf:resource="&comp;Source" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasGenesis">
    <rdfs:range rdf:resource="&prov;Genesis"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#genesisOf">
    <rdfs:domain rdf:resource="&prov;Genesis"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#used">
    <rdfs:domain rdf:resource="&proc;Process"/>
    <rdfs:range rdf:resource="&prov;Artifact"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#wasControlledBy">
    <rdfs:domain rdf:resource="&proc;Process"/>
    <rdfs:range rdf:resource="&prov;Artifact"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#wasDerivedFrom">
    <rdfs:domain rdf:resource="&prov;Artifact"/>
    <rdfs:range rdf:resource="&prov;Artifact"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#wasGeneratedAt">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#wasGeneratedBy">
    <rdfs:domain rdf:resource="&prov;Artifact"/>
    <rdfs:range rdf:resource="&proc;Process"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#wasOperatedBy">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#wasTriggeredBy">
    <rdfs:domain rdf:resource="&proc;Process"/>
    <rdfs:range rdf:resource="&proc;Process"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#wasPerformedAt">
    <rdfs:domain rdf:resource="&proc;Process"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#wasPerformedBy">
    <rdfs:domain rdf:resource="&proc;Process"/>
    <rdfs:range rdf:resource="&prov;Agent"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#wasEncodedBy">
    <rdfs:domain rdf:resource="&prov;Artifact"/>
    <rdfs:range rdf:resource="&prov;Artifact"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#conclusion">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasRule">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasConclusion">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasInferenceRule">
    <rdfs:range rdf:resource="&prov;InferenceRule"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasVariableMapping">
    <rdfs:range rdf:resource="&prov;VariableMapping"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hadProcess">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#fillValue">
    <rdfs:range rdf:resource="&prod;FillValue"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#bgcolor"/>

  <owl:ObjectProperty rdf:about="#transparent"/>

  <owl:ObjectProperty rdf:about="#hasDataModel">
    <rdfs:subPropertyOf rdf:resource="&mrela;representationOf"/>
    <rdfs:range rdf:resource="&data;DataModel"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasDataService">
    <rdfs:range rdf:resource="&serv;DataService"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#reductionIn">
    <rdfs:range rdf:resource="&prop;Property"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasProcessingLevel">
    <rdfs:range rdf:resource="&ordi;ProcessingLevel"/>
  </owl:ObjectProperty>

</rdf:RDF>
