<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY func "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprMathFunction.owl">
  <!ENTITY ener "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propEnergy.owl">
  <!ENTITY pres "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propPressure.owl">
  <!ENTITY spac "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propSpaceMultidimensional.owl">
  <!ENTITY thic "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propSpaceThickness.owl">
  <!ENTITY mass "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propMass.owl">
  <!ENTITY chem "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propChemical.owl">
  <!ENTITY temp "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propTemperature.owl">
  <!ENTITY prela "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/relaPhysical.owl">
  <!ENTITY sfunc "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/reprSciFunction.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF 
   xml:base = "&sfunc;"
   xmlns:func = "&func;"
   xmlns:spac = "&spac;"
   xmlns:thic = "&thic;"
   xmlns:chem = "&chem;"
   xmlns:sfunc = "&sfunc;"
   xmlns:prela = "&prela;"
   xmlns:ener = "&ener;"
   xmlns:mass = "&mass;"
   xmlns:temp = "&temp;"
   xmlns:pres = "&pres;"
   xmlns:owl = "&owl;"
   xmlns:rdf = "&rdf;"
   xmlns:rdfs = "&rdfs;"
   xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&func;"/>
     <owl:imports rdf:resource="&prela;"/>
     <owl:imports rdf:resource="&ener;"/>
     <owl:imports rdf:resource="&pres;"/>
     <owl:imports rdf:resource="&mass;"/>
     <owl:imports rdf:resource="&spac;"/>
     <owl:imports rdf:resource="&thic;"/>
     <owl:imports rdf:resource="&chem;"/>
     <owl:imports rdf:resource="&temp;"/>
  </owl:Ontology>

<!-- Scientific -->
  <owl:Class rdf:about="#Invariant">
    <rdfs:subClassOf rdf:resource="&func;#Function"/>
  </owl:Class>

  <owl:Class rdf:about="#Isopleth">
    <rdfs:subClassOf rdf:resource="#Invariant" />
  </owl:Class>

  <owl:Class rdf:about="#Isentrope">
    <rdfs:subClassOf rdf:resource="#Isopleth"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasConstantQuantity"/>
        <owl:allValuesFrom rdf:resource="&ener;#Entropy"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Isobar">
    <rdfs:subClassOf rdf:resource="#Isopleth"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasConstantQuantity"/>
        <owl:allValuesFrom rdf:resource="&pres;#Pressure"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Isochore">
    <rdfs:subClassOf rdf:resource="#Isopleth"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasConstantQuantity"/>
        <owl:allValuesFrom rdf:resource="&spac;#Volume"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Isohaline">
    <rdfs:subClassOf rdf:resource="#Isopleth"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasConstantQuantity"/>
        <owl:allValuesFrom rdf:resource="&chem;#Salinity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Isotere">
    <rdfs:subClassOf rdf:resource="#Isopleth"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasConstantQuantity"/>
        <owl:allValuesFrom rdf:resource="&mass;#Density"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Isotherm">
    <rdfs:subClassOf rdf:resource="#Isopleth"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasConstantQuantity"/>
        <owl:allValuesFrom rdf:resource="&temp;#Temperature"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ThicknessLine">
    <rdfs:subClassOf rdf:resource="#Isopleth" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasConstantQuantity" />
        <owl:allValuesFrom rdf:resource="&thic;#Thickness" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">A line drawn through all geographic points at which the thickness of a given atmospheric layer is the same; an isopleth of thickness.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#AdiabaticInvariant">
    <rdfs:subClassOf rdf:resource="#Invariant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasConstantQuantity"/>
        <owl:allValuesFrom rdf:resource="&ener;#Entropy"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

</rdf:RDF>
