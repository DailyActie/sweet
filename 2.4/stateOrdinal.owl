<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY ordi "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propOrdinal.owl#">
  <!ENTITY ostate "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/stateOrdinal.owl#">
  <!ENTITY screla "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/relaSci.owl#">  
  <!ENTITY repr "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/repr.owl#">  
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&ostate;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:repr="&repr;"
         xmlns:ordi="&ordi;"
         xmlns:ostate="&ostate;"
         xmlns:screla="&screla;"
>

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/relaSci.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/propOrdinal.owl"/>
     <owl:imports rdf:resource="https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/repr.owl"/>
  </owl:Ontology>
  
<!-- Ordinal Properties -->
  <ordi:MineralAssessment rdf:about="#Recoverable"/>

  <ordi:MineralAssessment rdf:about="#Paramarginal"/>

  <ordi:MineralAssessment rdf:about="#Submarginal"/>

<!-- Qualifiers -->
  <ordi:Qualifier rdf:about="#Acute">
  </ordi:Qualifier>

  <ordi:Qualifier rdf:about="#Almost">
  </ordi:Qualifier>

  <ordi:Qualifier rdf:about="#Extremely">
    <screla:moreExtensiveThan rdf:resource="#Very"/>
  </ordi:Qualifier>

  <ordi:Qualifier rdf:about="#Middle">
    <owl:sameAs rdf:resource="#Medium"/>
    <owl:sameAs rdf:resource="#Intermediate"/>
  </ordi:Qualifier>
  <ordi:Qualifier rdf:about="#Medium"/>
  <ordi:Qualifier rdf:about="#Intermediate"/>

  <ordi:Qualifier rdf:about="#Optimal">
    <screla:moreExtensiveThan rdf:resource="#Slightly"/>
  </ordi:Qualifier>

  <ordi:Qualifier rdf:about="#Semi">
  </ordi:Qualifier>

  <ordi:Qualifier rdf:about="#Slight">
    <owl:sameAs rdf:resource="#Slightly"/>
  </ordi:Qualifier>
  <ordi:Qualifier rdf:about="#Slightly"/>

  <ordi:Qualifier rdf:about="#Suboptimal">
  </ordi:Qualifier>

  <ordi:Qualifier rdf:about="#Total">
  </ordi:Qualifier>

  <ordi:Qualifier rdf:about="#Transitional">
  </ordi:Qualifier>

  <ordi:Qualifier rdf:about="#Well">
  </ordi:Qualifier>

  <ordi:Qualifier rdf:about="#Ultra">
  </ordi:Qualifier>
  
  <ordi:Qualifier rdf:about="#Very">
  </ordi:Qualifier>

</rdf:RDF>
