<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  
  <!ENTITY juri "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/humanJurisdiction.owl">
  <!ENTITY comm "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/humanCommerce.owl">
  <!ENTITY deci "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/humanDecision.owl">
  <!ENTITY rese "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/humanResearch.owl">
  <!ENTITY indu "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/matrIndustrial.owl">
  <!ENTITY equi "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/matrEquipment.owl">
  <!ENTITY hrela "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/relaHuman.owl">
  <!ENTITY know "https://raw.githubusercontent.com/ESIPFed/sweet/master/2.4/humanKnowledgeDomain.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF 
   xml:base = "&hrela;"
   xmlns:hrela = "&hrela;"
   xmlns:know = "&know;"
   xmlns:equi = "&equi;"
   xmlns:comm = "&comm;"
   xmlns:deci = "&deci;"
   xmlns:indu = "&indu;"
   xmlns:rese = "&rese;"
   xmlns:juri = "&juri;"
   xmlns:owl = "&owl;"
   xmlns:rdf = "&rdf;"
   xmlns:rdfs = "&rdfs;"
   xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.4">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&know;"/>
     <owl:imports rdf:resource="&equi;"/>
     <owl:imports rdf:resource="&indu;"/>
     <owl:imports rdf:resource="&rese;"/>
     <owl:imports rdf:resource="&juri;"/>
     <owl:imports rdf:resource="&deci;"/>
     <owl:imports rdf:resource="&comm;"/>
  </owl:Ontology>

<!-- human -->
  <owl:DatatypeProperty rdf:about="#hasTRL">
    <rdfs:range rdf:resource="&xsd;integer"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="#produces">
    <rdfs:subPropertyOf rdf:resource="#hasHumanActivity"/>
    <rdfs:range rdf:resource="&indu;#Product"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#readinessOf">
    <rdfs:range rdf:resource="&equi;#Infrastructure"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#successIn">
    <rdfs:range rdf:resource="&rese;#Research" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#studyOf">
    <rdfs:domain rdf:resource="&know;#KnowledgeDomain"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasJurisdiction">
    <rdfs:subPropertyOf rdf:resource="#hasHumanActivity"/>
    <rdfs:range rdf:resource="&juri;#Jurisdiction"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasAttribute">
    <rdfs:subPropertyOf rdf:resource="#hasHumanActivity"/>
    <rdfs:range rdf:resource="&deci;#Attribute" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasDecisionMaker">
    <rdfs:subPropertyOf rdf:resource="#hasHumanActivity"/>
    <rdfs:range rdf:resource="&deci;#DecisionMaker" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasObjective">
    <rdfs:subPropertyOf rdf:resource="#hasHumanActivity"/>
    <rdfs:range rdf:resource="&deci;#Objective" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasLandUse">
    <rdfs:range rdf:resource="&comm;#LandUse"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasHumanActivity"/>

</rdf:RDF>
