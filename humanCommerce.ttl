@prefix comm: <http://sweetontology.net/humanCommerce/> .
@prefix cons: <http://sweetontology.net/humanEnvirConservation/> .
@prefix hrela: <http://sweetontology.net/relaHuman/> .
@prefix huma: <http://sweetontology.net/human/> .
@prefix infr: <http://sweetontology.net/matrEquipment/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sweetontology.net/humanCommerce>
  rdf:type owl:Ontology ;
  rdfs:label "SWEET Ontology" ;
  owl:imports <http://sweetontology.net/human> ;
  owl:imports <http://sweetontology.net/humanEnvirConservation> ;
  owl:imports <http://sweetontology.net/matrEquipment> ;
  owl:imports <http://sweetontology.net/relaHuman> ;
  owl:versionInfo "3.1.0" ;
.
comm:Assembly
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Production ;
.
comm:CapacityBuiiding
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:ClearCutting
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Forestry ;
.
comm:Construction
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Production ;
.
comm:ConsumptiveUse
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:Demand
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:Development
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:Dig
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Extraction ;
.
comm:Drill
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Extraction ;
.
comm:EngineeringActivity
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Production ;
.
comm:Exploration
  rdf:type owl:Class ;
  rdfs:subClassOf comm:EngineeringActivity ;
.
comm:Extraction
  rdf:type owl:Class ;
  rdfs:subClassOf comm:EngineeringActivity ;
  owl:equivalentClass comm:ResourceExtraction ;
.
comm:Forestry
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:FossilFuelBurning
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:Grading
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Processing ;
.
comm:InSitu
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
  owl:equivalentClass comm:OnSite ;
.
comm:Industrialization
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:LandUse
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:Manafacturing
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Production ;
.
comm:Management
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:ManagementSystem
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom comm:Management ;
      owl:onProperty hrela:hasHumanActivity ;
    ] ;
.
comm:Mining
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Extraction ;
.
comm:OnSite
  rdf:type owl:Class ;
.
comm:Packaging
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Production ;
.
comm:Processing
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Production ;
.
comm:Production
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:Productivity
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
comm:Prospecting
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Exploration ;
.
comm:ResourceExtraction
  rdf:type owl:Class ;
.
comm:StripMining
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Mining ;
.
comm:SustainableDevelopment
  rdf:type owl:Class ;
  rdfs:subClassOf comm:Development ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom cons:Sustainable ;
      owl:onProperty hrela:hasHumanActivity ;
    ] ;
.
comm:Urbanization
  rdf:type owl:Class ;
  rdfs:subClassOf huma:HumanActivity ;
.
