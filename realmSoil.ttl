@prefix cate: <http://sweetontology.net/propCategorical/> .
@prefix cstate: <http://sweetontology.net/statePhysical/> .
@prefix mix: <http://sweetontology.net/phenMixing/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix realm: <http://sweetontology.net/realm/> .
@prefix rela: <http://sweetontology.net/rela/> .
@prefix soil: <http://sweetontology.net/realmSoil/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sweetontology.net/realmSoil>
  rdf:type owl:Ontology ;
  rdfs:label "SWEET Ontology" ;
  owl:imports <http://sweetontology.net/phenMixing> ;
  owl:imports <http://sweetontology.net/propCategorical> ;
  owl:imports <http://sweetontology.net/realm> ;
  owl:imports <http://sweetontology.net/rela> ;
  owl:imports <http://sweetontology.net/relaSpace> ;
  owl:imports <http://sweetontology.net/statePhysical> ;
  owl:versionInfo "3.1.0" ;
.
soil:A_Horizon
  rdf:type owl:Class ;
.
soil:ActiveLayer
  rdf:type owl:Class ;
  rdfs:comment "In environments containing permafrost, the active layer is the top layer of soil that thaws during the summer and freezes again during the autumn."@en ;
  rdfs:subClassOf soil:SoilLayer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty rela:partOf ;
      owl:someValuesFrom soil:Permafrost ;
    ] ;
.
soil:Alfisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:Andisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:Aridisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:B_Horizon
  rdf:type owl:Class ;
.
soil:C_Horizon
  rdf:type owl:Class ;
.
soil:Cryosol
  rdf:type owl:Class ;
  owl:equivalentClass soil:Gelisol ;
.
soil:Entisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:Gelisol
  rdf:type owl:Class ;
  rdfs:comment "Gelisols are soils of very cold climates which are defined as containing permafrost within two metres of the soil surface. The word Gelisol comes from the Latin gelare meaning to freeze, a reference to the process of cryoturbation that occurs from the alternating thawing and freezing characteristic of Gelisols."@en ;
  rdfs:subClassOf soil:SoilOrder ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom mix:Cryoturbation ;
      owl:onProperty rela:hasProcess ;
    ] ;
.
soil:Ground
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue cstate:Solid ;
      owl:onProperty rela:hasState ;
    ] ;
.
soil:Histisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:Inceptisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:Mollisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:O_Horizon
  rdf:type owl:Class ;
.
soil:OrganicMatter
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty srela:above ;
      owl:someValuesFrom soil:P_Horizon ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty srela:above ;
      owl:someValuesFrom soil:Topsoil ;
    ] ;
  owl:equivalentClass soil:O_Horizon ;
.
soil:Oxisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:P_Horizon
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
.
soil:Paleosol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:ParentRock
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
  owl:equivalentClass soil:C_Horizon ;
.
soil:Permafrost
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue cstate:Frozen ;
      owl:onProperty rela:hasState ;
    ] ;
.
soil:Regolith
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
.
soil:SoilHorizon
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
.
soil:SoilLayer
  rdf:type owl:Class ;
  rdfs:subClassOf realm:PlanetaryLayer ;
.
soil:SoilOrder
  rdf:type owl:Class ;
  rdfs:subClassOf cate:Classification ;
.
soil:Spodosol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:Subsoil
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty srela:above ;
      owl:someValuesFrom soil:ParentRock ;
    ] ;
  owl:equivalentClass soil:B_Horizon ;
.
soil:Substrata
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
  owl:equivalentClass soil:Substratum ;
.
soil:Substratum
  rdf:type owl:Class ;
.
soil:SurfaceSoil
  rdf:type owl:Class ;
.
soil:Talik
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom soil:Permafrost ;
      owl:onProperty rela:partOf ;
    ] ;
.
soil:Topsoil
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilLayer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty srela:above ;
      owl:someValuesFrom soil:Subsoil ;
    ] ;
  owl:equivalentClass soil:A_Horizon ;
  owl:equivalentClass soil:SurfaceSoil ;
.
soil:Ultisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
soil:Vertisol
  rdf:type owl:Class ;
  rdfs:subClassOf soil:SoilOrder ;
.
