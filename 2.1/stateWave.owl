<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [

  <!ENTITY root "http://sweet.jpl.nasa.gov/2.1/">
  <!ENTITY repr "&root;repr.owl#">
  <!ENTITY math "&root;reprMath.owl#">
  <!ENTITY tscale "&root;reprTimeScale.owl#">
  <!ENTITY state "&root;state.owl#">
  <!ENTITY wave "&root;stateWave.owl#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&wave;"
         xmlns:repr="&repr;"
         xmlns:tscale="&tscale;"
         xmlns:state="&state;"
         xmlns:wave="&wave;"
         xmlns:math="&math;"
         xmlns:xsd="&xsd;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="1.1">
     <owl:imports rdf:resource="&root;reprMath.owl"/>
     <owl:imports rdf:resource="&root;reprTimeScale.owl"/>
     <owl:imports rdf:resource="&root;repr.owl"/>
     <owl:imports rdf:resource="&root;state.owl"/>
  </owl:Ontology>

  <owl:Class rdf:about="#WaveState">
    <rdfs:subClassOf rdf:resource="&state;State"/>
  </owl:Class>

  <wave:WaveState rdf:about="#Eddy">
  </wave:WaveState>

  <wave:WaveState rdf:about="#Zonal">
    <owl:differentFrom rdf:resource="#Eddy" />
  </wave:WaveState>

  <wave:WaveState rdf:about="#Constructive">
  </wave:WaveState>

  <wave:WaveState rdf:about="#Destructive">
    <owl:differentFrom rdf:resource="#Constructive" />
  </wave:WaveState>

  <wave:WaveState rdf:about="#HighFrequency">
  </wave:WaveState>

  <wave:WaveState rdf:about="#LowFrequency">
    <owl:differentFrom rdf:resource="#HighFrequency" />
  </wave:WaveState>

  <wave:WaveState rdf:about="#Filtered">
  </wave:WaveState>

</rdf:RDF>
