<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY root "http://sweet.jpl.nasa.gov/2.1/">
  <!ENTITY rela "&root;reprMathRelation.owl#">
  <!ENTITY scale "&root;reprTimeScale.owl#">
  <!ENTITY tstate "&root;stateTime.owl#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&tstate;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rela="&rela;"
         xmlns:scale="&scale;"
         xmlns:tstate="&tstate;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="1.1">
     <owl:imports rdf:resource="&root;reprMathRelation.owl"/>
     <owl:imports rdf:resource="&root;reprTimeScale.owl"/>
  </owl:Ontology>

<!-- frequency scale -->
  <owl:ObjectProperty rdf:about="#moreFrequentThan">
    <rdfs:subPropertyOf rdf:resource="&rela;greaterThan"/>
    <rdfs:domain rdf:resource="&scale;Frequency"/>
    <rdfs:range rdf:resource="&scale;Frequency"/>
  </owl:ObjectProperty>

  <scale:Frequency rdf:about="#Frequent">
    <tstate:moreFrequentThan rdf:resource="#Infrequent"/>
    <tstate:moreFrequentThan rdf:resource="#Rare"/>
  </scale:Frequency>

  <scale:Frequency rdf:about="#Consistent">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Usual">
    <tstate:moreFrequentThan rdf:resource="#Unusual"/>
  </scale:Frequency>

  <scale:Frequency rdf:about="#Unusual">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Episodic">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Erratic">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Infrequent">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Rare">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Spasmodic">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Steady">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Accidental">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Common">
    <tstate:moreFrequentThan rdf:resource="#Rare"/>
  </scale:Frequency>

  <scale:Frequency rdf:about="#Interrupted">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Cyclic">
  </scale:Frequency>

  <scale:Frequency rdf:about="#Secular">
    <owl:differentFrom rdf:resource="#Cyclic"/>
  </scale:Frequency>

  <scale:Frequency rdf:about="#Uninterrupted">
    <owl:differentFrom rdf:resource="#Interrupted"/>
  </scale:Frequency>

  <scale:TemporalScale rdf:about="#Stationary">
  </scale:TemporalScale>

  <scale:TemporalScale rdf:about="#NonStationary">
    <owl:differentFrom rdf:resource="#Stationary"/>
  </scale:TemporalScale>

</rdf:RDF>
