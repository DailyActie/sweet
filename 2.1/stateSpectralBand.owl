  <!ENTITY freq "http://sweet.jpl.nasa.gov/2.1/propTimeFrequency.owl">
  <!ENTITY freq2 "http://sweet.jpl.nasa.gov/2.1/propTimeFrequency.owl#">
  <!ENTITY band "http://sweet.jpl.nasa.gov/2.1/stateSpectralBand.owl#">
  <!ENTITY line "http://sweet.jpl.nasa.gov/2.1/stateSpectralLine.owl">
