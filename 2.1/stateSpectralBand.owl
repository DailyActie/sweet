  <!ENTITY units "http://sweet.jpl.nasa.gov/2.1/reprSciUnits.owl">
  <!ENTITY units2 "http://sweet.jpl.nasa.gov/2.1/reprSciUnits.owl#">
  <!ENTITY waves "http://sweet.jpl.nasa.gov/2.1/propSpaceDistance.owl">
  <!ENTITY waves2 "http://sweet.jpl.nasa.gov/2.1/propSpaceDistance.owl#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
