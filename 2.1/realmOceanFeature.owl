<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY root   "http://sweet.jpl.nasa.gov/2.1/">
  <!ENTITY math   "&root;reprMath.owl#">
  <!ENTITY geol   "&root;realmGeol.owl#">
  <!ENTITY ht     "&root;propSpaceHeight.owl#">
  <!ENTITY mult   "&root;propSpaceMultidimensional.owl#">
  <!ENTITY units  "&root;reprSciUnits.owl#">
  <!ENTITY planet "&root;realm.owl#">
  <!ENTITY body   "&root;realmAstroBody.owl#">
  <!ENTITY ocean  "&root;realmOcean.owl#">
  <!ENTITY feat  "&root;realmOceanFeature.owl#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&feat;"
         xmlns:owl="&owl;"
         xmlns:ht="&ht;"
         xmlns:planet="&planet;"
         xmlns:ocean="&ocean;"
         xmlns:math="&math;"
         xmlns:body="&body;"
         xmlns:feat="&feat;"
         xmlns:mult="&mult;"
         xmlns:geol="&geol;"
         xmlns:units="&units;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="1.1">
     <owl:imports rdf:resource="&root;reprMath.owl"/>
     <owl:imports rdf:resource="&root;realmGeol.owl"/>
     <owl:imports rdf:resource="&root;realmOcean.owl"/>
     <owl:imports rdf:resource="&root;propSpaceHeight.owl"/>
     <owl:imports rdf:resource="&root;propSpaceMultidimensional.owl"/>
     <owl:imports rdf:resource="&root;realm.owl"/>
     <owl:imports rdf:resource="&root;realmAstroBody.owl"/>
     <owl:imports rdf:resource="&root;reprSciUnits.owl"/>
  </owl:Ontology>

  <owl:Class rdf:about="#EarthOcean">
    <rdfs:subClassOf rdf:resource="&planet;Ocean" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&body;hasPlanet" />
        <owl:hasValue rdf:resource="&planet;Earth" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#OceanDepth">
    <rdfs:subClassOf rdf:resource="&ht;Depth" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&units;hasUnit" />
        <owl:hasValue rdf:resource="&units;meter" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#OceanArea">
    <rdfs:subClassOf rdf:resource="&mult;Area" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&units;hasUnit" />
        <owl:hasValue rdf:resource="&units;million_km2" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#OceanVolume">
    <rdfs:subClassOf rdf:resource="&mult;Volume" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&units;hasUnit" />
        <owl:hasValue rdf:resource="&units;million_km3" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <feat:EarthOcean rdf:about="#PacificOcean">
     <ht:hasAverageDepth rdf:resource="#4282m" />
     <ht:hasMaximumDepth rdf:resource="#10911m" />
     <mult:hasVolume rdf:resource="#707pt6million_km3" />
     <mult:hasArea rdf:resource="#165pt2million_km2" />
  </feat:EarthOcean>
  <feat:OceanDepth rdf:about="#4282m">
     <math:hasNumericValue rdf:datatype="&xsd;double">4282</math:hasNumericValue>
  </feat:OceanDepth>
  <feat:OceanDepth rdf:about="#10911m">
     <math:hasNumericValue rdf:datatype="&xsd;double">10911</math:hasNumericValue>
  </feat:OceanDepth>
  <feat:OceanVolume rdf:about="#707pt6million_km3">
     <math:hasNumericValue rdf:datatype="&xsd;double">707.6</math:hasNumericValue>
  </feat:OceanVolume>
  <feat:OceanArea rdf:about="#165pt2million_km2">
     <math:hasNumericValue rdf:datatype="&xsd;double">165.2</math:hasNumericValue>
  </feat:OceanArea>

  <feat:EarthOcean rdf:about="#AtlanticOcean">
     <ht:hasAverageDepth rdf:resource="#3926m" />
     <ht:hasMaximumDepth rdf:resource="#8605m" />
     <mult:hasVolume rdf:resource="#323pt6million_km3" />
     <mult:hasArea rdf:resource="#82pt4million_km2" />
  </feat:EarthOcean>
  <feat:OceanDepth rdf:about="#3926m">
     <math:hasNumericValue rdf:datatype="&xsd;double">3926</math:hasNumericValue>
  </feat:OceanDepth>
  <feat:OceanDepth rdf:about="#8605m">
     <math:hasNumericValue rdf:datatype="&xsd;double">8605</math:hasNumericValue>
  </feat:OceanDepth>
  <feat:OceanVolume rdf:about="#323pt6million_km3">
     <math:hasNumericValue rdf:datatype="&xsd;double">323.6</math:hasNumericValue>
  </feat:OceanVolume>
  <feat:OceanArea rdf:about="#82pt4million_km2">
     <math:hasNumericValue rdf:datatype="&xsd;double">82.4</math:hasNumericValue>
  </feat:OceanArea>

  <feat:EarthOcean rdf:about="#ArcticOcean">
     <ht:hasAverageDepth rdf:resource="#1038m" />
     <mult:hasArea rdf:resource="#14pt1million_km2" />
  </feat:EarthOcean>
  <feat:OceanDepth rdf:about="#1038m">
     <math:hasNumericValue rdf:datatype="&xsd;double">1038</math:hasNumericValue>
  </feat:OceanDepth>
  <feat:OceanArea rdf:about="#14pt1million_km2">
     <math:hasNumericValue rdf:datatype="&xsd;double">14.1</math:hasNumericValue>
  </feat:OceanArea>

  <feat:EarthOcean rdf:about="#IndianOcean">
     <ht:hasAverageDepth rdf:resource="#3963m" />
     <ht:hasMaximumDepth rdf:resource="#8047m" />
     <mult:hasVolume rdf:resource="#291pt0million_km3" />
     <mult:hasArea rdf:resource="#73pt4million_km2" />
  </feat:EarthOcean>
  <feat:OceanDepth rdf:about="#3963m">
     <math:hasNumericValue rdf:datatype="&xsd;double">3963</math:hasNumericValue>
  </feat:OceanDepth>
  <feat:OceanDepth rdf:about="#8047m">
     <math:hasNumericValue rdf:datatype="&xsd;double">8047</math:hasNumericValue>
  </feat:OceanDepth>
  <feat:OceanVolume rdf:about="#291pt0million_km3">
     <math:hasNumericValue rdf:datatype="&xsd;double">291.0</math:hasNumericValue>
  </feat:OceanVolume>
  <feat:OceanArea rdf:about="#73pt4million_km2">
     <math:hasNumericValue rdf:datatype="&xsd;double">73.4</math:hasNumericValue>
  </feat:OceanArea>

  <feat:EarthOcean rdf:about="#CaribbeanSea">
     <ht:hasMaximumDepth rdf:resource="#7686m" />
     <mult:hasArea rdf:resource="#2pt8million_km2" />
  </feat:EarthOcean>
  <feat:OceanDepth rdf:about="#7686m">
     <math:hasNumericValue rdf:datatype="&xsd;double">7686</math:hasNumericValue>
  </feat:OceanDepth>
  <feat:OceanArea rdf:about="#2pt8million_km2">
     <math:hasNumericValue rdf:datatype="&xsd;double">2.8</math:hasNumericValue>
  </feat:OceanArea>

  <feat:EarthOcean rdf:about="#SouthernOcean">
     <ht:hasMaximumDepth rdf:resource="#7235m" />
     <mult:hasArea rdf:resource="#20pt3million_km2" />
  </feat:EarthOcean>
  <feat:OceanDepth rdf:about="#7235m">
     <math:hasNumericValue rdf:datatype="&xsd;double">7235</math:hasNumericValue>
  </feat:OceanDepth>
  <feat:OceanArea rdf:about="#20pt3million_km2">
     <math:hasNumericValue rdf:datatype="&xsd;double">20.3</math:hasNumericValue>
  </feat:OceanArea>

</rdf:RDF>
