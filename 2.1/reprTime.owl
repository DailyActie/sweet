<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [

  <!ENTITY root "http://sweet.jpl.nasa.gov/2.1/">
  <!ENTITY repr "&root;repr.owl#">
  <!ENTITY reg "&root;realmRegion.owl#">
  <!ENTITY math "&root;reprMath.owl#">
  <!ENTITY rela "&root;reprMathRelation.owl#">
  <!ENTITY time "&root;reprTime.owl#">
  <!ENTITY units "&root;reprSciUnits.owl#">
  <!ENTITY owlt "http://www.w3.org/2006/time#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&time;"
         xmlns:repr="&repr;"
         xmlns:reg="&reg;"
         xmlns:time="&time;"
         xmlns:owlt="&owlt;"
         xmlns:units="&units;"
         xmlns:math="&math;"
         xmlns:rela="&rela;"
         xmlns:xsd="&xsd;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="1.1">
     <owl:imports rdf:resource="&root;reprMath.owl"/>
     <owl:imports rdf:resource="&root;reprMathRelation.owl"/>
     <owl:imports rdf:resource="&root;repr.owl"/>
     <owl:imports rdf:resource="&root;reprSciUnits.owl"/>
     <owl:imports rdf:resource="&root;realmRegion.owl"/>
  </owl:Ontology>

<!-- Time Entities -->
  <owl:Class rdf:about="#Duration">
    <owl:equivalentClass rdf:resource="#TemporalEntity"/>
  </owl:Class>

  <owl:Class rdf:about="#Instant">
    <rdfs:subClassOf rdf:resource="#TimeReference"/>
    <owl:equivalentClass rdf:resource="#Instance"/>
  </owl:Class>
  <owl:Class rdf:about="#Instance"/>

  <owl:Class rdf:about="#TemporalEntity">
    <rdfs:subClassOf rdf:resource="&math;NumericalEntity"/>
  </owl:Class>

  <owl:Class rdf:about="#TemporalReference">
    <rdfs:subClassOf rdf:resource="#TemporalEntity"/>
    <owl:equivalentClass rdf:resource="#TimeReference"/>
  </owl:Class>
  <owl:Class rdf:about="#TimeReference"/>

  <owl:Class rdf:about="#Time">
    <rdfs:subClassOf rdf:resource="&repr;Dimension"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;second"/>
        <owl:onProperty rdf:resource="&units;hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">This concept is of time as an independent variable</rdfs:comment>
  </owl:Class>

<!-- Diurnal definitions -->
  <owl:Class rdf:about="#Noon">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
  </owl:Class>

  <owl:Class rdf:about="#Day">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
  </owl:Class>

  <owl:Class rdf:about="#Daytime">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
  </owl:Class>

  <owl:Class rdf:about="#Midnight">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
  </owl:Class>

  <owl:Class rdf:about="#Nighttime">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
    <owl:equivalentClass rdf:resource="#Nocturnal"/>
  </owl:Class>
  <owl:Class rdf:about="#Nocturnal"/>

  <owl:Class rdf:about="#Evening">
    <rdfs:subClassOf rdf:resource="#Nighttime"/>
  </owl:Class>

  <owl:Class rdf:about="#Morning">
    <rdfs:subClassOf rdf:resource="#Daytime"/>
  </owl:Class>

  <owl:Class rdf:about="#Sunset">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Evening"/>
        <owl:onProperty rdf:resource="#startOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Sunrise">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Morning"/>
        <owl:onProperty rdf:resource="#startOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Dawn">
    <rdfs:subClassOf rdf:resource="#Twilight"/>
  </owl:Class>

  <owl:Class rdf:about="#Dusk">
    <rdfs:subClassOf rdf:resource="#Twilight"/>
  </owl:Class>

  <owl:Class rdf:about="#Twilight">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
  </owl:Class>

<!-- Season definitions -->
  <owl:Class rdf:about="#Equinox">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
  </owl:Class>

  <owl:Class rdf:about="#Season">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
  </owl:Class>

  <owl:Class rdf:about="#Solstice">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
  </owl:Class>

  <time:Season rdf:about="#Autumn">
    <owl:sameAs rdf:resource="#Fall"/>
    <owlt:hasBeginning rdf:resource="#AutumnalEquinox"/>
    <owlt:hasEnd rdf:resource="#WinterSolstice"/>
  </time:Season>
  <time:Season rdf:about="#Fall"/>

  <time:Season rdf:about="#Spring">
    <owlt:hasBeginning rdf:resource="#SpringEquinox"/>
    <owlt:hasEnd rdf:resource="#SummerSolstice"/>
  </time:Season>

  <time:Equinox rdf:about="#AutumnalEquinox">
    <owl:sameAs rdf:resource="#FallEquinox"/>
  </time:Equinox>
  <time:Equinox rdf:about="#FallEquinox"/>

  <time:Equinox rdf:about="#SpringEquinox">
    <owl:differentFrom rdf:resource="#FallEquinox"/>
  </time:Equinox>

  <time:Season rdf:about="#Summer">
    <owlt:hasBeginning rdf:resource="#SummerSolstice"/>
    <owlt:hasEnd rdf:resource="#FallEquinox"/>
  </time:Season>

  <time:Solstice rdf:about="#SummerSolstice">
    <time:hasNHTime rdf:resource="#JuneSolstice"/>
    <time:hasSHTime rdf:resource="#DecemberSolstice"/>
  </time:Solstice>

  <time:Solstice rdf:about="#JuneSolstice">
    <time:hasDayOfYear rdf:datatype="&xsd;nonNegativeInteger">172</time:hasDayOfYear>
  </time:Solstice>

  <time:Solstice rdf:about="#DecemberSolstice">
    <time:hasDayOfYear rdf:datatype="&xsd;nonNegativeInteger">355</time:hasDayOfYear>
  </time:Solstice>

  <time:Season rdf:about="#Winter">
    <owlt:hasBeginning rdf:resource="#WinterSolstice"/>
    <owlt:hasEnd rdf:resource="#SpringEquinox"/>
  </time:Season>

  <time:Solstice rdf:about="#WinterSolstice">
    <time:hasSHTime rdf:resource="#JuneSolstice"/>
    <time:hasNHTime rdf:resource="#DecemberSolstice"/>
  </time:Solstice>
        
<!-- Object Properties -->
  <owl:ObjectProperty rdf:ID="hasTime">
    <rdfs:range rdf:resource="#Time"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasNHTime">
    <rdfs:subPropertyOf rdf:resource="#hasTime"/>
    <rdfs:domain rdf:resource="&reg;NorthLatitudeBand"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasSHTime">
    <rdfs:subPropertyOf rdf:resource="#hasTime"/>
    <rdfs:domain rdf:resource="&reg;SouthLatitudeBand"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasTimeReference">
    <rdfs:range rdf:resource="#TimeReference"/>
    <owl:equivalentProperty rdf:resource="#hasTemporalReference"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasTemporalReference"/>

  <owl:DatatypeProperty rdf:ID="hasDayOfYear">
  </owl:DatatypeProperty>

<!-- Declarations of owl-time extensions -->
  <owl:Class rdf:about="&owlt;TemporalEntity">
    <rdfs:subClassOf rdf:resource="#Time"/>
  </owl:Class>
  <owl:Class rdf:about="&owlt;Instant">
    <rdfs:subClassOf rdf:resource="#Time"/>
  </owl:Class>
  <owl:Class rdf:about="&owlt;ProperInterval">
    <rdfs:subClassOf rdf:resource="#Time"/>
  </owl:Class>
  <owl:Class rdf:about="&owlt;Interval">
    <rdfs:subClassOf rdf:resource="#Time"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="&owlt;hasDurationDescription">
    <rdfs:subPropertyOf rdf:resource="#hasTimeReference"/>
  </owl:ObjectProperty>

<!-- Temporal relations -->
  <owl:ObjectProperty rdf:about="#temporalRelation">
    <rdfs:subPropertyOf rdf:resource="&rela;relation"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#temporalSubset">
    <rdfs:subPropertyOf rdf:resource="&rela;subsetOf"/>
    <rdfs:subPropertyOf rdf:resource="#temporalRelation"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#after">
    <rdfs:subPropertyOf rdf:resource="&rela;greaterThan"/>
    <rdfs:subPropertyOf rdf:resource="#temporalRelation"/>
    <owl:equivalentProperty rdf:resource="#follows"/>
    <owl:inverseOf rdf:resource="#before"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#follows"/>

  <owl:ObjectProperty rdf:about="#before">
    <rdfs:subPropertyOf rdf:resource="&rela;lessThan"/>
    <rdfs:subPropertyOf rdf:resource="#temporalRelation"/>
    <owl:equivalentProperty rdf:resource="#precedes"/>
    <owl:inverseOf rdf:resource="#after"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#precedes"/>

  <owl:ObjectProperty rdf:about="#followedBy">
    <rdfs:subPropertyOf rdf:resource="#before"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#follows">
    <rdfs:subPropertyOf rdf:resource="#after"/>
    <owl:inverseOf rdf:resource="#followedBy"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#dependentUpon">
    <rdfs:subPropertyOf rdf:resource="#after"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#startOf">
    <rdfs:subPropertyOf rdf:resource="#followedBy"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#endOf">
    <rdfs:subPropertyOf rdf:resource="#follows"/>
  </owl:ObjectProperty>

  <owl:Class rdf:about="#TimeFrame">
    <rdfs:subClassOf rdf:resource="&repr;ReferenceFrame"/>
    <rdfs:subClassOf rdf:resource="#Time"/>
  </owl:Class>
  
  <time:TimeFrame rdf:about="#Sidereal">
  </time:TimeFrame>

  <time:TimeFrame rdf:about="#Synodic">
  </time:TimeFrame>

  <owl:Class rdf:about="#TimeZone">
    <rdfs:subClassOf rdf:resource="#TemporalReference"/>
  </owl:Class>

  <time:TimeZone rdf:about="#UniversalTime">
    <owl:sameAs rdf:resource="#UT"/>
  </time:TimeZone>
  <time:TimeZone rdf:about="#UT"/>

  <owl:Class rdf:about="#LocalTime">
    <rdfs:subClassOf rdf:resource="#TimeZone"/>
  </owl:Class>

  <owl:Class rdf:about="#SolarTime">
    <rdfs:subClassOf rdf:resource="#LocalTime"/>
  </owl:Class>

  <owl:Class rdf:about="#Local">
    <rdfs:subClassOf rdf:resource="#LocalTime"/>
  </owl:Class>

</rdf:RDF>
