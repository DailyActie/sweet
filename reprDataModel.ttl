@prefix data: <http://sweetontology.net/reprDataProduct/> .
@prefix dmrepr: <http://sweetontology.net/reprDataModel/> .
@prefix mod: <http://sweetontology.net/reprSciModel/> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix obj: <http://sweetontology.net/reprSpaceGeometry/> .
@prefix obj3d: <http://sweetontology.net/reprSpaceGeometry3D/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix repr: <http://sweetontology.net/repr/> .
@prefix space: <http://sweetontology.net/reprSpace/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix time: <http://sweetontology.net/reprTime/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sweetontology.net/reprDataModel>
  rdf:type owl:Ontology ;
  rdfs:label "SWEET Ontology" ;
  owl:imports <http://sweetontology.net/relaMath> ;
  owl:imports <http://sweetontology.net/relaSpace> ;
  owl:imports <http://sweetontology.net/repr> ;
  owl:imports <http://sweetontology.net/reprDataProduct> ;
  owl:imports <http://sweetontology.net/reprSciModel> ;
  owl:imports <http://sweetontology.net/reprSpace> ;
  owl:imports <http://sweetontology.net/reprSpaceGeometry> ;
  owl:imports <http://sweetontology.net/reprSpaceGeometry3D> ;
  owl:imports <http://sweetontology.net/reprTime> ;
  owl:versionInfo "3.1.0" ;
.
dmrepr:Array
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataStructure ;
.
dmrepr:Cell
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataModel ;
.
dmrepr:Character
  rdf:type owl:Class ;
  rdfs:subClassOf repr:Representation ;
.
dmrepr:Column
  rdf:type owl:Class ;
  rdfs:subClassOf repr:Dimension ;
.
dmrepr:DataModel
  rdf:type owl:Class ;
  rdfs:subClassOf mod:Model ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom data:Data ;
      owl:onProperty mrela:representationOf ;
    ] ;
.
dmrepr:DataStructure
  rdf:type owl:Class ;
  rdfs:subClassOf repr:Representation ;
.
dmrepr:Delimiter
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:Character ;
.
dmrepr:Field
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataModel ;
.
dmrepr:Grid
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataModel ;
  rdfs:subClassOf dmrepr:Raster ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty srela:hasGeometricalObject ;
      owl:someValuesFrom obj:Rectangle ;
    ] ;
.
dmrepr:Increment
  rdf:type owl:Class ;
  rdfs:subClassOf repr:Scale ;
.
dmrepr:Parameter
  rdf:type owl:Class ;
  rdfs:subClassOf repr:Dimension ;
.
dmrepr:Point
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataModel ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom obj:Point ;
      owl:onProperty srela:hasGeometricalObject ;
    ] ;
.
dmrepr:Raster
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataModel ;
.
dmrepr:ReducedGrid
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:Grid ;
.
dmrepr:Resolution
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:Increment ;
.
dmrepr:Row
  rdf:type owl:Class ;
  rdfs:subClassOf repr:Dimension ;
.
dmrepr:SpatialRepresentation
  rdf:type owl:Class ;
  rdfs:subClassOf repr:Representation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom space:Space ;
      owl:onProperty mrela:representationOf ;
    ] ;
.
dmrepr:Structure
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataStructure ;
.
dmrepr:Swath
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataModel ;
.
dmrepr:Swath_2D
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:Swath ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom obj:GeometricalObject_2D ;
      owl:onProperty srela:hasGeometricalObject ;
    ] ;
.
dmrepr:Swath_3D
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:Swath ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom obj3d:GeometricalObject_3D ;
      owl:onProperty srela:hasGeometricalObject ;
    ] ;
.
dmrepr:TemporalRepresentation
  rdf:type owl:Class ;
  rdfs:subClassOf repr:Representation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom time:Time ;
      owl:onProperty mrela:representationOf ;
    ] ;
.
dmrepr:Trajectory
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataModel ;
.
dmrepr:Vector
  rdf:type owl:Class ;
  rdfs:subClassOf dmrepr:DataModel ;
.
