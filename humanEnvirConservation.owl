<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY root "http://sweetontology.net/">
  <!ENTITY com "&root;humanCommerce#">
  <!ENTITY human "&root;human#">
  <!ENTITY realm "&root;realm#">
  <!ENTITY rela "&root;rela#">
  <!ENTITY prot "&root;humanEnvirConservation#">
  <!ENTITY prop "&root;prop#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&prot;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:com="&com;"
         xmlns:human="&human;"
         xmlns:realm="&realm;"
         xmlns:rela="&rela;"
         xmlns:prot="&prot;"
         xmlns:prop="&prop;"
>

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.4">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&root;prop"/>
     <owl:imports rdf:resource="&root;humanCommerce"/>
     <owl:imports rdf:resource="&root;human"/>
     <owl:imports rdf:resource="&root;realm"/>
     <owl:imports rdf:resource="&root;rela"/>
  </owl:Ontology>

  <owl:Class rdf:about="#LandPreservation">
    <rdfs:subClassOf rdf:resource="#LandManagement"/>
  </owl:Class>

  <owl:Class rdf:about="#Protection">
    <rdfs:subClassOf rdf:resource="#Sustainability"/>
    <rdfs:subClassOf rdf:resource="&prop;Role"/>
  </owl:Class>

  <owl:Class rdf:about="#Conservation">
    <rdfs:subClassOf rdf:resource="&com;Management"/>
  </owl:Class>

  <owl:Class rdf:about="#LandManagement">
    <rdfs:subClassOf rdf:resource="&com;Management"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;hasRealm" />
        <owl:someValuesFrom rdf:resource="&realm;Land" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#LandTenure">
    <rdfs:subClassOf rdf:resource="#LandManagement"/>
  </owl:Class>

  <owl:Class rdf:about="#HabitatManagement">
    <rdfs:subClassOf rdf:resource="#LandManagement"/>
  </owl:Class>

  <owl:Class rdf:about="#Stewardship">
    <rdfs:subClassOf rdf:resource="&com;Management"/>
  </owl:Class>

  <owl:Class rdf:about="#Sustainable">
    <rdfs:subClassOf rdf:resource="&human;HumanActivity"/>
    <owl:equivalentClass rdf:resource="#Sustainability"/>
  </owl:Class>
  <owl:Class rdf:about="#Sustainability"/>

</rdf:RDF>
