<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY chem "https://raw.githubusercontent.com/ESIPFed/sweet/master/matr.owl"> 
  <!ENTITY rela "https://raw.githubusercontent.com/ESIPFed/sweet/master/rela.owl"> 
  <!ENTITY srela "https://raw.githubusercontent.com/ESIPFed/sweet/master/relaSpace.owl"> 
  <!ENTITY crela "https://raw.githubusercontent.com/ESIPFed/sweet/master/relaChemical.owl"> 
  <!ENTITY prela "https://raw.githubusercontent.com/ESIPFed/sweet/master/relaPhysical.owl"> 
  <!ENTITY phen "https://raw.githubusercontent.com/ESIPFed/sweet/master/phen.owl"> 
  <!ENTITY geol "https://raw.githubusercontent.com/ESIPFed/sweet/master/phenGeol.owl"> 
  <!ENTITY geolr "https://raw.githubusercontent.com/ESIPFed/sweet/master/realmGeol.owl"> 
  <!ENTITY petr "https://raw.githubusercontent.com/ESIPFed/sweet/master/matrRock.owl"> 
  <!ENTITY ign "https://raw.githubusercontent.com/ESIPFed/sweet/master/matrRockIgneous.owl"> 
  <!ENTITY geom "https://raw.githubusercontent.com/ESIPFed/sweet/master/reprSpaceGeometry.owl"> 
  <!ENTITY dir "https://raw.githubusercontent.com/ESIPFed/sweet/master/propSpaceDirection.owl"> 
  <!ENTITY basin "https://raw.githubusercontent.com/ESIPFed/sweet/master/realmGeolBasin.owl"> 
  <!ENTITY def "https://raw.githubusercontent.com/ESIPFed/sweet/master/phenSolid.owl"> 
  <!ENTITY geomor "https://raw.githubusercontent.com/ESIPFed/sweet/master/phenGeolGeomorphology.owl"> 
  <!ENTITY oro "https://raw.githubusercontent.com/ESIPFed/sweet/master/realmGeolOrogen.owl">
  <!ENTITY tec "https://raw.githubusercontent.com/ESIPFed/sweet/master/realmLandTectonic.owl">
  <!ENTITY tecp "https://raw.githubusercontent.com/ESIPFed/sweet/master/phenGeolTectonic.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">

]>

<rdf:RDF xml:base="&oro;"
         xmlns:phen="&phen;"
         xmlns:petr="&petr;"
         xmlns:tecp="&tecp;"
         xmlns:rela="&rela;"
         xmlns:srela="&srela;"
         xmlns:crela="&crela;"
         xmlns:prela="&prela;"
         xmlns:ign="&ign;"
         xmlns:geol="&geol;"
         xmlns:geolr="&geolr;"
         xmlns:dir="&dir;"
         xmlns:basin="&basin;"
         xmlns:def="&def;"
         xmlns:geomor="&geomor;"
         xmlns:chem="&chem;"
         xmlns:geom="&geom;"
         xmlns:oro="&oro;"
         xmlns:tec="&tec;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
	 xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.4">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&chem;"/>
     <owl:imports rdf:resource="&phen;"/>
     <owl:imports rdf:resource="&petr;"/>
     <owl:imports rdf:resource="&tecp;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&srela;"/>
     <owl:imports rdf:resource="&crela;"/>
     <owl:imports rdf:resource="&prela;"/>
     <owl:imports rdf:resource="&ign;"/>
     <owl:imports rdf:resource="&tec;"/>
     <owl:imports rdf:resource="&geol;"/>
     <owl:imports rdf:resource="&geolr;"/>
     <owl:imports rdf:resource="&dir;"/>
     <owl:imports rdf:resource="&basin;"/>
     <owl:imports rdf:resource="&def;"/>
     <owl:imports rdf:resource="&geomor;"/>
     <owl:imports rdf:resource="&geom;"/>
  </owl:Ontology>

<!-- Realms -->
  <owl:Class rdf:about="#Arc">
    <rdfs:subClassOf rdf:resource="&geom;#Arc"/>
    <rdfs:subClassOf rdf:resource="&geolr;#GeologicFeature"/>
  </owl:Class>

  <owl:Class rdf:about="#BackArc">
    <rdfs:subClassOf rdf:resource="#Arc"/>
    <rdfs:comment xml:lang="en">Back-arc basins (or retro-arc basins) are geologic features, submarine basins associated with island arcs and subduction zones. They are found at some convergent plate boundaries, presently concentrated in the Western Pacific ocean. [Wikipedia]</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Orogen">
    <rdfs:subClassOf rdf:resource="&geolr;#GeologicProvince"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasPlanetaryStructure"/>
        <owl:someValuesFrom rdf:resource="&tec;#ThrustSystem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&geol;#Orogeny"/>
        <owl:onProperty rdf:resource="&rela;#hasPhenomena"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&tecp;#Terrane"/>
          <owl:onProperty rdf:resource="&prela;#hasPlanetaryStructure"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&crela;#hasSubstance"/>
        <owl:someValuesFrom rdf:resource="&petr;#Melange"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasPhenomena"/>
        <owl:someValuesFrom rdf:resource="&geol;#OrogenicCollapse"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&tec;#Suture"/>
        <owl:onProperty rdf:resource="&prela;#hasPlanetaryStructure"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasPhenomena"/>
       <owl:someValuesFrom rdf:resource="&def;#Deformation"/>
       </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasPlanetaryStructure"/>
        <owl:someValuesFrom rdf:resource="&basin;#ForelandBasin"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ForeArc">
    <rdfs:subClassOf rdf:resource="#Arc"/>
    <rdfs:subClassOf rdf:resource="&tec;#SupraSubductionZoneComplex"/>
    <rdfs:comment xml:lang="en">A forearc is a depression in the sea floor located between a subduction zone and an associated volcanic arc. It is typically filled with sediments from the adjacent landmass and the island arc in addition to trapped oceanic crustal material.  [Wikipedia]</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#MagmaticArc">
    <rdfs:subClassOf rdf:resource="#Arc"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasPlanetaryStructure"/>
        <owl:someValuesFrom rdf:resource="#ForeArc"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#RearArc"/>
        <owl:onProperty rdf:resource="&prela;#hasPlanetaryStructure"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#hasAxis"/>
        <owl:allValuesFrom rdf:resource="#ArcAxis"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#RemnantArc">
    <rdfs:subClassOf rdf:resource="#Arc"/>
    <rdfs:comment xml:lang="en">The remnant arc is what is left on the rear side of the speading zone as athe basin broadens. [Wikipedia]</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#RearArc">
    <owl:equivalentClass rdf:resource="#BackArc"/>
  </owl:Class>

  <owl:Class rdf:about="#RetroArc">
    <owl:equivalentClass rdf:resource="#BackArc"/>
  </owl:Class>

  <owl:Class rdf:about="#ArcAxis">
    <rdfs:subClassOf rdf:resource="&dir;#Axis"/>
    <rdfs:comment xml:lang="en">In a Back-arc basin the arc axis is the line where spreading occurs on the overlying plate of the subduction zone. [Wikipedia]</rdfs:comment>
  </owl:Class>

</rdf:RDF>
